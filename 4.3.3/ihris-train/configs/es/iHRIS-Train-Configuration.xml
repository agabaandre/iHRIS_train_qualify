<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="ihris-train" path="/I2CE">
  <metadata>
    <version>4.3.3.0.1</version>
  </metadata>
  <configurationGroup name="ihris-train" path="/I2CE" locale="es">
    <version>4.3.0.1</version>
    <configurationGroup name="page">
      <configurationGroup name="manage">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="es" type="string">
            <value>Gestionar personas</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="search_reports" path="/modules/CustomReports/search_reports">
        <configuration name="search_people" values="many" type="delimited" locale="es">
          <value>name:Búsqueda de personas</value>
          <value>description:Buscar cualquier registro de persona en el sistema para verlo, imprimirlo o actualizarlo.</value>
        </configuration>
        <configuration name="search_training_provider" values="many" type="delimited" locale="es">
          <value>name:Búsqueda de proveedores de capacitación</value>
          <value>description:Buscar cualquier proveedor de capacitación en el sistema para verlo, imprimirlo o actualizarlo.</value>
        </configuration>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>