<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="ihris-train" path="/I2CE">
  <metadata>
    <version>4.3.3.0.1</version>
  </metadata>
  <configurationGroup name="ihris-train" path="/I2CE" locale="de">
    <version>4.3.0.1</version>
    <configurationGroup name="page">
      <configurationGroup name="manage">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="de" type="string">
            <value>Mitarbeiter verwalten</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="search_reports" path="/modules/CustomReports/search_reports">
        <configuration name="search_people" values="many" type="delimited" locale="de">
          <value>name:Mitarbeiter suchen</value>
          <value>description:Datensatz zu jeglichen Personen im System, zum Überprüfen, Aktualisierung oder Drucken, ermitteln.</value>
        </configuration>
        <configuration name="search_training_provider" values="many" type="delimited" locale="de">
          <value>name:Suche Schulungsanbieter</value>
          <value>description:Jeglichen Schulungsanbieter im System, zum Überprüfen, Aktualisierung oder Drucken, ermitteln.</value>
        </configuration>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>