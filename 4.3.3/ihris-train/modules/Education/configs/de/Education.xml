<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="Education" path="/I2CE">
  <metadata>
    <version>4.3.0.1</version>
  </metadata>
  <configurationGroup name="Education" path="/I2CE" locale="de">
    <version>4.3.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="de">
      <configuration name="can_edit_database_list_academic_level" locale="de">
        <value>Die Liste Studienniveaus bearbeiten</value>
      </configuration>
      <configuration name="can_view_database_list_academic_level" locale="de">
        <value>Die Liste der Studienniveaus anzeigen</value>
      </configuration>
      <configuration name="can_edit_database_list_qualification" locale="de">
        <value>Die Qualifikationsliste bearbeiten</value>
      </configuration>
      <configuration name="can_view_database_list_qualification" locale="de">
        <value>Die Qualifikationsliste anzeigen</value>
      </configuration>
      <configuration name="person_can_view_child_form_education" locale="de">
        <value>Kann das Datenblatt mit den Ausbildungsinformationen zu einer Person einsehen</value>
      </configuration>
      <configuration name="person_can_edit_child_form_education" locale="de">
        <value>Kann das Datenblatt mit den Ausbildungsinformationen zu einer Person bearbeiten</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="education">
          <configuration name="display" values="single" locale="de">
            <value>Ausbildungsangaben</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="academic_level">
          <configuration name="display" values="single" locale="de">
            <value>Studienniveau</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="qualification">
          <configuration name="display" values="single" locale="de">
            <value>Qualifikation</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_Education">
          <configurationGroup name="fields">
            <configurationGroup name="academic_level">
              <configuration name="headers" type="delimited" locale="de">
                <value>default:Studienniveau</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="qualification">
              <configuration name="headers" type="delimited" locale="de">
                <value>default:Qualifikation</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="year">
              <configuration name="headers" type="delimited" locale="de">
                <value>default:Jahr des Abschlusses</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="school">
              <configuration name="headers" type="delimited" locale="de">
                <value>default:Name der Einrichtung</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>