<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="Education" path="/I2CE">
  <metadata>
    <version>4.3.0.1</version>
  </metadata>
  <configurationGroup name="Education" path="/I2CE" locale="es">
    <version>4.3.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="es">
      <configuration name="can_edit_database_list_academic_level" locale="es">
        <value>Editar la lista de nivel académico</value>
      </configuration>
      <configuration name="can_view_database_list_academic_level" locale="es">
        <value>Ver la lista de nivel académico</value>
      </configuration>
      <configuration name="can_edit_database_list_qualification" locale="es">
        <value>Editar la lista de clasificación</value>
      </configuration>
      <configuration name="can_view_database_list_qualification" locale="es">
        <value>Ver la lista de clasificación</value>
      </configuration>
      <configuration name="person_can_view_child_form_education" locale="es">
        <value>Puede ver el formulario secundario de educación de una persona</value>
      </configuration>
      <configuration name="person_can_edit_child_form_education" locale="es">
        <value>Puede editar el formulario secundario de educación de una persona</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="education">
          <configuration name="display" values="single" locale="es">
            <value>Información educativa</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="academic_level">
          <configuration name="display" values="single" locale="es">
            <value>Nivel académico</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="qualification">
          <configuration name="display" values="single" locale="es">
            <value>Calificación</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_Education">
          <configurationGroup name="fields">
            <configurationGroup name="academic_level">
              <configuration name="headers" type="delimited" locale="es">
                <value>default:Nivel académico</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="qualification">
              <configuration name="headers" type="delimited" locale="es">
                <value>default:Calificación</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="year">
              <configuration name="headers" type="delimited" locale="es">
                <value>default:Año de graduación</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="school">
              <configuration name="headers" type="delimited" locale="es">
                <value>default:Nombre de la institución</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>