<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="Education" path="/I2CE">
  <metadata>
    <version>4.3.0.1</version>
  </metadata>
  <configurationGroup name="Education" path="/I2CE" locale="nl">
    <version>4.3.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="nl">
      <configuration name="can_edit_database_list_academic_level" locale="nl"></configuration>
      <configuration name="can_view_database_list_academic_level" locale="nl"></configuration>
      <configuration name="can_edit_database_list_qualification" locale="nl"></configuration>
      <configuration name="can_view_database_list_qualification" locale="nl"></configuration>
      <configuration name="person_can_view_child_form_education" locale="nl"></configuration>
      <configuration name="person_can_edit_child_form_education" locale="nl"></configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="education">
          <configuration name="display" values="single" locale="nl"></configuration>
        </configurationGroup>
        <configurationGroup name="academic_level">
          <configuration name="display" values="single" locale="nl"></configuration>
        </configurationGroup>
        <configurationGroup name="qualification">
          <configuration name="display" values="single" locale="nl">
            <value>Kwalificatie</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_Education">
          <configurationGroup name="fields">
            <configurationGroup name="academic_level">
              <configuration name="headers" type="delimited" locale="nl"></configuration>
            </configurationGroup>
            <configurationGroup name="qualification">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:Kwalificatie</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="year">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:Afstudeerjaar</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="school">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:Naam van de instelling</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>