<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="CustomReports-full-search_person" path="/modules/CustomReports">
  <metadata>
    <version>4.3.0</version>
  </metadata>
  <configurationGroup name="CustomReports-full-search_person" path="/modules/CustomReports" locale="en_US">
    <version>4.3.0</version>
    <configurationGroup name="search_person" path="//modules/CustomReports/relationships/search_person">
      <configuration name="display_name" locale="en_US">
        <value>Search Person</value>
      </configuration>
      <configuration name="description" locale="en_US">
        <value>Search Relationship for participants</value>
      </configuration>
      <configurationGroup name="joins">
        <configurationGroup name="demographic">
          <configuration name="description" locale="en_US">
            <value>Shows the demographic information of the person</value>
          </configuration>
          <configuration name="display_name" locale="en_US">
            <value>Demographic</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="position">
          <configuration name="description" locale="en_US">
            <value>Shows the position information of the person</value>
          </configuration>
          <configuration name="display_name" locale="en_US">
            <value>Position</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="registraion">
          <configuration name="description" locale="en_US">
            <value>Shows the registration information of the person</value>
          </configuration>
          <configuration name="display_name" locale="en_US">
            <value>Registration</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="search_person" path="//modules/CustomReports/reports/search_person">
      <configurationGroup name="meta">
        <configuration name="display_name" locale="en_US">
          <value>Search Person</value>
        </configuration>
        <configuration name="description" locale="en_US">
          <value>Search all people's records</value>
        </configuration>
      </configurationGroup>
      <configurationGroup name="reporting_forms">
        <configurationGroup name="primary_form">
          <configurationGroup name="fields">
            <configurationGroup name="firstname">
              <configurationGroup name="limits">
                <configurationGroup name="contains">
                  <configuration name="header" locale="en_US">
                    <value>First Name</value>
                  </configuration>
                </configurationGroup>
              </configurationGroup>
              <configuration name="header" locale="en_US">
                <value>First Name</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="nationality">
              <configurationGroup name="limits">
                <configurationGroup name="equals">
                  <configuration name="header" locale="en_US">
                    <value>Nationality</value>
                  </configuration>
                </configurationGroup>
              </configurationGroup>
              <configuration name="header" locale="en_US">
                <value>Nationality</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="othername">
              <configuration name="header" locale="en_US">
                <value>Other Names</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="surname">
              <configurationGroup name="limits">
                <configurationGroup name="contains">
                  <configuration name="header" locale="en_US">
                    <value>Surname</value>
                  </configuration>
                </configurationGroup>
              </configurationGroup>
              <configuration name="header" locale="en_US">
                <value>Surname</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="village">
              <configuration name="header" locale="en_US">
                <value>Home Village</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="registraion">
          <configurationGroup name="fields">
            <configurationGroup name="council">
              <configurationGroup name="limits">
                <configurationGroup name="equals">
                  <configuration name="header" locale="en_US">
                    <value>Registration Council</value>
                  </configuration>
                </configurationGroup>
              </configurationGroup>
              <configuration name="header" locale="en_US">
                <value>Registration Council</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="license_expiration">
              <configuration name="header" locale="en_US">
                <value>License Expiration Date</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="license_number">
              <configuration name="header" locale="en_US">
                <value>License Number</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="registration_date">
              <configuration name="header" locale="en_US">
                <value>Registration Date</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="registration_number">
              <configuration name="header" locale="en_US">
                <value>Registration Number</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="position">
          <configurationGroup name="fields">
            <configurationGroup name="health_facility">
              <configurationGroup name="limits">
                <configurationGroup name="equals">
                  <configuration name="header" locale="en_US">
                    <value>Health Facility</value>
                  </configuration>
                </configurationGroup>
              </configurationGroup>
              <configuration name="header" locale="en_US">
                <value>Health Facility</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="job">
              <configurationGroup name="limits">
                <configurationGroup name="equals">
                  <configuration name="header" locale="en_US">
                    <value>Job/Post Title</value>
                  </configuration>
                </configurationGroup>
                <configurationGroup name="in">
                  <configuration name="header" locale="en_US">
                    <value>Job/Post Title</value>
                  </configuration>
                </configurationGroup>
              </configurationGroup>
              <configuration name="header" locale="en_US">
                <value>Job/Post Title</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="demographic">
          <configurationGroup name="fields">
            <configurationGroup name="birth_date">
              <configuration name="header" locale="en_US">
                <value>Date of Birth</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="gender">
              <configurationGroup name="limits">
                <configurationGroup name="equals">
                  <configuration name="header" locale="en_US">
                    <value>Gender</value>
                  </configuration>
                </configurationGroup>
              </configurationGroup>
              <configuration name="header" locale="en_US">
                <value>Gender</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="search_people" path="//modules/CustomReports/reportViews/search_people">
      <configuration name="description" locale="en_US">
        <value>Search all people in the system</value>
      </configuration>
      <configuration name="display_name" locale="en_US">
        <value>Search Person</value>
      </configuration>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>