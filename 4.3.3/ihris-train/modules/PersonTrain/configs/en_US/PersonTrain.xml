<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="PersonTrain" path="/I2CE">
  <metadata>
    <version>4.3.0</version>
  </metadata>
  <configurationGroup name="PersonTrain" path="/I2CE" locale="en_US">
    <version>4.3.0</version>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="person">
          <configuration name="display" values="single" locale="en_US">
            <value>Person</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_TrainPerson">
          <configurationGroup name="fields">
            <configurationGroup name="village">
              <configuration name="headers" type="delimited" values="many" locale="en_US">
                <value>default:Home Village</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>