<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="accident" path="/I2CE">
  <metadata>
    <version>4.2.2.0.1</version>
  </metadata>
  <configurationGroup name="accident" path="/I2CE" locale="ja">
    <version>4.2.0.1</version>
    <configurationGroup name="invalid_formfield_message" path="/modules/forms/formfield_messages" locale="ja">
      <configuration name="accident_date" path="accident/end_date/bad_date">
        <value>終了日は開始日より後にする必要があります。</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description">
      <configuration name="person_can_edit_child_form_accident" locale="ja">
        <value>職場事故・負傷に指定された人を編集できます。</value>
      </configuration>
      <configuration name="person_can_view_child_form_accident" locale="ja">
        <value>職場事故・負傷に指定された人を表示できます。</value>
      </configuration>
      <configuration name="can_edit_database_list_accident_type" locale="ja">
        <value>職場事故タイプ一覧を編集します。</value>
      </configuration>
      <configuration name="can_view_database_list_accident_type" locale="ja">
        <value>職場事故タイプ一覧を表示します。</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="accident">
          <configuration name="display" values="single" locale="ja">
            <value>事故</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="accident_type">
          <configuration name="display" values="single" locale="ja">
            <value>職場事故タイプ</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_Accident">
          <configurationGroup name="fields">
            <configurationGroup name="occurence_date">
              <configuration name="headers" type="delimited" locale="ja">
                <value>default:発生年月日</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="accident_type">
              <configuration name="headers" type="delimited" locale="ja">
                <value>default:事故タイプ</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="start_date">
              <configuration name="headers" type="delimited" locale="ja">
                <value>default:適用の開始</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="end_date">
              <configuration name="headers" type="delimited" locale="ja">
                <value>default:適用の終了</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="followup">
              <configuration name="headers" type="delimited" locale="ja">
                <value>default:必要なフォロー アップ</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="persons_involved">
              <configuration name="headers" type="delimited" locale="ja">
                <value>default:関係者</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="home" path="/modules/accident/page/home">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="ja">
            <value>職場事故を追加/更新</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>