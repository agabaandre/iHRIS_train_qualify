<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="accident" path="/I2CE">
  <metadata>
    <version>4.2.2.0.1</version>
  </metadata>
  <configurationGroup name="accident" path="/I2CE" locale="oc">
    <version>4.2.0.1</version>
    <configurationGroup name="invalid_formfield_message" path="/modules/forms/formfield_messages" locale="oc">
      <configuration name="accident_date" path="accident/end_date/bad_date">
        <value>La data de fin deu èsser posteriora a la data de començament.</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description">
      <configuration name="person_can_edit_child_form_accident" locale="oc">
        <value>Pòt editar las nafraduras e accidents sul luòc de trabalh atribuits a una persona</value>
      </configuration>
      <configuration name="person_can_view_child_form_accident" locale="oc">
        <value>Pòt afichar las nafraduras e accidents sul luòc de trabalh atribuits a una persona</value>
      </configuration>
      <configuration name="can_edit_database_list_accident_type" locale="oc">
        <value>Editar la lista dels tipes d’accidents sul luòc de trabalh</value>
      </configuration>
      <configuration name="can_view_database_list_accident_type" locale="oc">
        <value>Afichar la lista dels tipes d'accident de trabalh</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="accident">
          <configuration name="display" values="single" locale="oc"></configuration>
        </configurationGroup>
        <configurationGroup name="accident_type">
          <configuration name="display" values="single" locale="oc">
            <value>Tipe d’accident sul luòc de trabalh</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_Accident">
          <configurationGroup name="fields">
            <configurationGroup name="occurence_date">
              <configuration name="headers" type="delimited" locale="oc">
                <value>default:Data de l'eveniment</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="accident_type">
              <configuration name="headers" type="delimited" locale="oc">
                <value>default:Tipe d’accident</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="start_date">
              <configuration name="headers" type="delimited" locale="oc">
                <value>default:Començament d’aplicabilitat</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="end_date">
              <configuration name="headers" type="delimited" locale="oc">
                <value>default:Fin d’aplicabilitat</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="followup">
              <configuration name="headers" type="delimited" locale="oc">
                <value>default:Seguiment requesit</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="persons_involved">
              <configuration name="headers" type="delimited" locale="oc">
                <value>default:Personas implicadas</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="home" path="/modules/accident/page/home">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="oc">
            <value>Apondre/Actualizar un accident sul luòc de trabalh</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>