<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="accident" path="/I2CE">
  <metadata>
    <version>4.2.2.0.1</version>
  </metadata>
  <configurationGroup name="accident" path="/I2CE" locale="da">
    <version>4.2.0.1</version>
    <configurationGroup name="invalid_formfield_message" path="/modules/forms/formfield_messages" locale="da">
      <configuration name="accident_date" path="accident/end_date/bad_date"></configuration>
    </configurationGroup>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description">
      <configuration name="person_can_edit_child_form_accident" locale="da">
        <value>Kan redigere skader og ulykker på arbejdspladser tilknyttet en person</value>
      </configuration>
      <configuration name="person_can_view_child_form_accident" locale="da">
        <value>Kan se skader og ulykker på arbejdspladser tilknyttet en person</value>
      </configuration>
      <configuration name="can_edit_database_list_accident_type" locale="da">
        <value>Rediger listen med typer af arbejdsulykker</value>
      </configuration>
      <configuration name="can_view_database_list_accident_type" locale="da"></configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="accident">
          <configuration name="display" values="single" locale="da">
            <value>Ulykke</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="accident_type">
          <configuration name="display" values="single" locale="da">
            <value>Type arbejdsulykke</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_Accident">
          <configurationGroup name="fields">
            <configurationGroup name="occurence_date">
              <configuration name="headers" type="delimited" locale="da"></configuration>
            </configurationGroup>
            <configurationGroup name="accident_type">
              <configuration name="headers" type="delimited" locale="da"></configuration>
            </configurationGroup>
            <configurationGroup name="start_date">
              <configuration name="headers" type="delimited" locale="da"></configuration>
            </configurationGroup>
            <configurationGroup name="end_date">
              <configuration name="headers" type="delimited" locale="da"></configuration>
            </configurationGroup>
            <configurationGroup name="followup">
              <configuration name="headers" type="delimited" locale="da">
                <value>default:Opfølgning påkrævet</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="persons_involved">
              <configuration name="headers" type="delimited" locale="da">
                <value>default:Folk involveret</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="home" path="/modules/accident/page/home">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="da">
            <value>Tilføj/opdater arbejdsulykke</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>