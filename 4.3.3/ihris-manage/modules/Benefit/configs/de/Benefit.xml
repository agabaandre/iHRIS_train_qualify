<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="ihris-manage-Benefit" path="/I2CE">
  <metadata>
    <version>4.3.0.1</version>
  </metadata>
  <configurationGroup name="ihris-manage-Benefit" path="/I2CE" locale="de">
    <version>4.3.0.1</version>
    <configurationGroup name="invalid_formfield_message" path="/modules/forms/formfield_messages" locale="de">
      <configuration name="benefit_date" path="benefit/end_date/bad_date">
        <value>Das Enddatum muss nach dem Anfangsdatum liegen.</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="de">
      <configuration name="person_can_view_child_form_benefit" locale="de">
        <value>Kann das zu den Zusatzleistungen einer Person gehörende Datenblatt einsehen</value>
      </configuration>
      <configuration name="person_can_edit_child_form_benefit" locale="de">
        <value>Kann das zu den Zusatzleistungen einer Person gehörende Datenblatt bearbeiten</value>
      </configuration>
      <configuration name="can_edit_database_list_benefit_type" locale="de">
        <value>Kann die Datenbank mit den Zusatzleistungsarten bearbeiten</value>
      </configuration>
      <configuration name="can_view_database_list_benefit_type" locale="de">
        <value>Kann die Datenbank mit den Zusatzleistungsarten einsehen</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="formsData" path="formsData/forms">
      <configurationGroup name="benefit_recurrence">
        <configurationGroup name="once">
          <configuration name="fields" value="many" type="delimited" locale="de">
            <value>name:Einmalig</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="weekly">
          <configuration name="fields" value="many" type="delimited" locale="de">
            <value>name:Wöchentlich</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="monthly">
          <configuration name="fields" value="many" type="delimited" locale="de">
            <value>name:Monatlich</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="yearly">
          <configuration name="fields" value="many" type="delimited" locale="de">
            <value>name:Jährlich</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="benefit">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="de">
            <value>Zusatzleistungen hinzufügen oder aktualisieren</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="benefit">
          <configuration name="display" values="single" locale="de">
            <value>Zusatzleistung</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="benefit_type">
          <configuration name="display" values="single" locale="de">
            <value>Zusatzleistungsart</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="benefit_recurrence">
          <configuration name="display" values="single" locale="de">
            <value>Intervall für die Zusatzleistung</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_Benefit">
          <configurationGroup name="fields">
            <configurationGroup name="type">
              <configuration name="headers" type="delimited" locale="de">
                <value>default:Zusatzleistungsart</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="amount">
              <configuration name="headers" type="delimited" locale="de">
                <value>default:Betrag</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="source">
              <configuration name="headers" type="delimited" locale="de">
                <value>default:Träger</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="start_date">
              <configuration name="headers" type="delimited" locale="de">
                <value>default:Anfangsdatum</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="end_date">
              <configuration name="headers" type="delimited" locale="de">
                <value>default:Enddatum</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="recurrence">
              <configuration name="headers" type="delimited" locale="de">
                <value>default:Intervallfrequenz</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>