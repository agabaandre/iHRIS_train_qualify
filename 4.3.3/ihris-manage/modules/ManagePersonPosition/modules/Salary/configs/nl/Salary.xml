<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="ihris-manage-Salary" path="/I2CE">
  <metadata>
    <version>4.3.0.1</version>
  </metadata>
  <configurationGroup name="ihris-manage-Salary" path="/I2CE" locale="nl">
    <version>4.3.0.1</version>
    <configurationGroup name="invalid_formfield_message" path="/modules/forms/formfield_messages" locale="nl">
      <configuration name="sal_bad_date" path="salary/start_date/bad_date">
        <value>De begindatum moet na de begindatum van het huidige salaris liggen.</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="salary">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="nl">
            <value>Salariswijziging</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="salary_history">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="nl">
            <value>Salarisgeschiedenis</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description">
      <configuration name="person_can_edit_child_form_salary" locale="nl">
        <value>Kan het salaris van een persoon bewerken</value>
      </configuration>
      <configuration name="person_can_delete_child_form_salary" locale="nl">
        <value>Kan het salaris van een persoon verwijderen</value>
      </configuration>
      <configuration name="person_can_change_child_form_salary" locale="nl">
        <value>Kan het bestaande salaris van een persoon wijzigen</value>
      </configuration>
      <configuration name="person_can_view_child_form_salary" locale="nl">
        <value>Kan het bestaande salaris van een persoon bekijken</value>
      </configuration>
      <configuration name="can_edit_database_list_salary_source" locale="nl">
        <value>De lijst salary_source bewerken</value>
      </configuration>
      <configuration name="can_view_database_list_salary_source" locale="nl">
        <value>De lijst salary_source bekijken</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="salary">
          <configuration name="display" values="single" locale="nl">
            <value>Salaris</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="salary_source">
          <configuration name="display" values="single" locale="nl">
            <value>Salarisbron</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_Position">
          <configurationGroup name="fields">
            <configurationGroup name="proposed_salary">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:Voorgesteld salaris</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="source">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:Bron</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="iHRIS_Salary">
          <configurationGroup name="fields">
            <configurationGroup name="start_date">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:Begindatum</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="salary">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:Salaris</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="end_date">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:Einddatum</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="notes">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:Opmerkingen</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>