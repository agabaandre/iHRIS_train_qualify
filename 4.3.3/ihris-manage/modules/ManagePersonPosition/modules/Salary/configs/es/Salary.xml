<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="ihris-manage-Salary" path="/I2CE">
  <metadata>
    <version>4.3.0.1</version>
  </metadata>
  <configurationGroup name="ihris-manage-Salary" path="/I2CE" locale="es">
    <version>4.3.0.1</version>
    <configurationGroup name="invalid_formfield_message" path="/modules/forms/formfield_messages" locale="es">
      <configuration name="sal_bad_date" path="salary/start_date/bad_date">
        <value>La fecha inicial debe ser posterior a la fecha inicial del salario actual.</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="salary">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="es">
            <value>Cambiar salario</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="salary_history">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="es">
            <value>Historia salarial</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description">
      <configuration name="person_can_edit_child_form_salary" locale="es">
        <value>Puede editar el salario de una persona</value>
      </configuration>
      <configuration name="person_can_delete_child_form_salary" locale="es">
        <value>Puede borrar el salario de una persona</value>
      </configuration>
      <configuration name="person_can_change_child_form_salary" locale="es">
        <value>Puede cambiar un salario existente de una persona</value>
      </configuration>
      <configuration name="person_can_view_child_form_salary" locale="es">
        <value>Puede ver un salario existente de una persona</value>
      </configuration>
      <configuration name="can_edit_database_list_salary_source" locale="es">
        <value>Editar fuente de la lista de _salario</value>
      </configuration>
      <configuration name="can_view_database_list_salary_source" locale="es">
        <value>Mostrar fuente de la lista de _salario</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="salary">
          <configuration name="display" values="single" locale="es">
            <value>Salario</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="salary_source">
          <configuration name="display" values="single" locale="es">
            <value>Fuente de ingresos</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_Position">
          <configurationGroup name="fields">
            <configurationGroup name="proposed_salary">
              <configuration name="headers" type="delimited" locale="es">
                <value>default:Salario propuesto</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="source">
              <configuration name="headers" type="delimited" locale="es">
                <value>default:Fuente</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="iHRIS_Salary">
          <configurationGroup name="fields">
            <configurationGroup name="start_date">
              <configuration name="headers" type="delimited" locale="es">
                <value>default:Fecha de Inicio</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="salary">
              <configuration name="headers" type="delimited" locale="es">
                <value>default:Salario</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="end_date">
              <configuration name="headers" type="delimited" locale="es">
                <value>default:Fecha de finalizaci√≥n</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="notes">
              <configuration name="headers" type="delimited" locale="es">
                <value>default:Notas</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>