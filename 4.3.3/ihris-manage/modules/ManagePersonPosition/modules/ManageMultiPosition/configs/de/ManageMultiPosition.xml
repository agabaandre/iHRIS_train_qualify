<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="ihris-manage-MultiPosition" path="/I2CE">
  <metadata>
    <version>4.2.2.0.1</version>
  </metadata>
  <configurationGroup name="ihris-manage-MultiPosition" path="/I2CE" locale="de">
    <version>4.2.0.1</version>
    <configurationGroup name="invalid_formfield_message" path="/modules/forms/formfield_messages" locale="de">
      <configuration name="code_req" path="position/code_start/required">
        <value>Wenn sie mehrere Positionen hinzufügen, müssen sie eine Zahl für den Code eingeben mit dem sie anfangen möchten.</value>
      </configuration>
      <configuration name="code_req" path="position/code_format/unique">
        <value>Ein Positionscode existiert bereits für eine der Positionen die sie hinzuzufügen versuchen. Bitte wählen sie ein anderes Code-Format oder beginnen sie mit einer höheren Nummer.</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_MultiPosition">
          <configurationGroup name="fields">
            <configurationGroup name="count">
              <configuration name="headers" type="delimited" locale="de">
                <value>default:Anzahl zu erstellender Positionen</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="code_start">
              <configuration name="headers" type="delimited" locale="de">
                <value>default:Numerierung mit dieser Zahl beginnen</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="code_format">
              <configuration name="headers" type="delimited" locale="de">
                <value>default:Code-Format zur Verwendung mit der Nummer</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>