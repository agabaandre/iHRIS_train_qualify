<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="ihris-manage-MultiPosition" path="/I2CE">
  <metadata>
    <version>4.2.2.0.1</version>
  </metadata>
  <configurationGroup name="ihris-manage-MultiPosition" path="/I2CE" locale="sco">
    <version>4.2.0.1</version>
    <configurationGroup name="invalid_formfield_message" path="/modules/forms/formfield_messages" locale="sco">
      <configuration name="code_req" path="position/code_start/required">
        <value>Gif ye'r eikin monie poseetions, than ye maun enter ae nummer fer the code tae stairt wi.</value>
      </configuration>
      <configuration name="code_req" path="position/code_format/unique">
        <value>Ae poseetion code awreadie exeests fer yin o the poseetions that ye'r ettlin tae eik. Please select anither code format or stairt wi ae heier nummer.</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_MultiPosition">
          <configurationGroup name="fields">
            <configurationGroup name="count">
              <configuration name="headers" type="delimited" locale="sco">
                <value>default:Nummer o Poseetions tae Cr√¶ft</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="code_start">
              <configuration name="headers" type="delimited" locale="sco">
                <value>default:The Nummer tae Stairt With fer the Poseetion Code</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="code_format">
              <configuration name="headers" type="delimited" locale="sco">
                <value>default:Code Format tae uise wi the Nummer</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>