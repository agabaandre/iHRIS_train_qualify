<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="ihris-manage-Job" path="/I2CE">
  <metadata>
    <version>4.2.2.0.1</version>
  </metadata>
  <configurationGroup name="ihris-manage-Job" path="/I2CE" locale="ru">
    <version>4.2.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description">
      <configuration name="can_edit_database_list_salary_grade" locale="ru">
        <value>Редактировать список градации зарплат</value>
      </configuration>
      <configuration name="can_view_database_list_salary_grade" locale="ru">
        <value>Посмотреть список градации зарплат</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="view_job">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="ru">
            <value>Просмотреть вакансию</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="salary_grade">
          <configuration name="display" values="single" locale="ru">
            <value>Градация зарплат</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_SalaryGrade">
          <configurationGroup name="fields">
            <configurationGroup name="name">
              <configuration name="headers" type="delimited" locale="ru">
                <value>default:Имя</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="start">
              <configuration name="headers" type="delimited" locale="ru">
                <value>default:Начало</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="end">
              <configuration name="headers" type="delimited" locale="ru">
                <value>default:Конец</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="midpoint">
              <configuration name="headers" type="delimited" locale="ru">
                <value>default:Середина</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="notes">
              <configuration name="headers" type="delimited" locale="ru">
                <value>default:Примечания</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="iHRIS_ManageJob">
          <configurationGroup name="fields">
            <configurationGroup name="salary_grade">
              <configuration name="headers" type="delimited" locale="ru">
                <value>default:Градация зарплат</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>