<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="ihris-manage-Job" path="/I2CE">
  <metadata>
    <version>4.3.0.1</version>
  </metadata>
  <configurationGroup name="ihris-manage-Job" path="/I2CE" locale="tl">
    <version>4.3.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description">
      <configuration name="can_edit_database_list_salary_grade" locale="tl">
        <value>Baguhin ang tala ng antas_ng_sahod</value>
      </configuration>
      <configuration name="can_view_database_list_salary_grade" locale="tl">
        <value>Tingnan ang tala ng antas_ng_sahod</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="view_job">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="tl">
            <value>Tingnan ang Hanapbuhay</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="salary_grade">
          <configuration name="display" values="single" locale="tl">
            <value>Antas ng Sahod</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_SalaryGrade">
          <configurationGroup name="fields">
            <configurationGroup name="name">
              <configuration name="headers" type="delimited" locale="tl">
                <value>default:Pangalan</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="start">
              <configuration name="headers" type="delimited" locale="tl">
                <value>default:Simula</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="end">
              <configuration name="headers" type="delimited" locale="tl">
                <value>default:Wakas</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="midpoint">
              <configuration name="headers" type="delimited" locale="tl">
                <value>default:Kalagitnaan</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="notes">
              <configuration name="headers" type="delimited" locale="tl">
                <value>default:Mga tala</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="iHRIS_ManageJob">
          <configurationGroup name="fields">
            <configurationGroup name="salary_grade">
              <configuration name="headers" type="delimited" locale="tl">
                <value>default:Antas ng Sahod</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>