<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="disciplinary_action" path="/I2CE">
  <metadata>
    <version>4.3.0.1</version>
  </metadata>
  <configurationGroup name="disciplinary_action" path="/I2CE" locale="tl">
    <version>4.3.0.1</version>
    <configurationGroup name="invalid_formfield_message" path="/modules/forms/formfield_messages" locale="tl">
      <configuration name="disciplinary_date" path="disciplinary_action/end_date/bad_date">
        <value>Ang petsa ng pagwawakas ay dapat na maging pagkalipas ng petsa ng pagsisimula.</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description">
      <configuration name="person_can_edit_child_form_disciplinary_action" locale="tl">
        <value>Mababago ang mga kapinsalaang pangpook ng hanabuhay at mga hakbang_sa_pagdidisiplina ng isang tao</value>
      </configuration>
      <configuration name="person_can_view_child_form_disciplinary_action" locale="tl">
        <value>Matitingnan ang mga kapinsalaang pampook ng hanapbuhay at mga hakbang_sa_pagdisiplina ng isang tao</value>
      </configuration>
      <configuration name="can_edit_database_list_disciplinary_action_type" locale="tl">
        <value>Baguhin ang tala ng uri ng hakbang_sa_pagdidisiplinang pampook ng hanapbuhay</value>
      </configuration>
      <configuration name="can_view_database_list_disciplinary_action_type" locale="tl">
        <value>Tingnan ang tala ng uri ng hakbanging_pangdisiplina sa pook ng hanapbuhay</value>
      </configuration>
      <configuration name="can_edit_database_list_disciplinary_action_reason" locale="tl">
        <value>Baguhin ang tala ng uri ng dahilan ng hakbang sa pagdidisiplinang pampook ng hanapbuhay</value>
      </configuration>
      <configuration name="can_view_database_list_disciplinary_action_reason" locale="tl">
        <value>Tingnan ang tala ng uri ng dahilan sa hakbang na pangdisiplina sa pook ng hanapbuhay</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="disciplinary_action">
          <configuration name="display" values="single" locale="tl">
            <value>Hakbanging Pangdisiplina</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="disciplinary_action_type">
          <configuration name="display" values="single" locale="tl">
            <value>Uri ng Hakbanging Pangdisiplina</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="disciplinary_action_reason">
          <configuration name="display" values="single" locale="tl">
            <value>Dahilan ng Hakbang na Pangdisiplina</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_DisciplinaryAction">
          <configurationGroup name="fields">
            <configurationGroup name="action_date">
              <configuration name="headers" type="delimited" locale="tl">
                <value>default:Petsa ng Talakayan</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="disciplinary_action_type">
              <configuration name="headers" type="delimited" locale="tl">
                <value>default:Ginawang Hakbang</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="disciplinary_action_reason">
              <configuration name="headers" type="delimited" locale="tl">
                <value>default:Dahilan ng Kilos</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="start_date">
              <configuration name="headers" type="delimited" locale="tl">
                <value>default:Simula ng Pagpapatupad</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="end_date">
              <configuration name="headers" type="delimited" locale="tl">
                <value>default:Wakas ng Pagpapatupad</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="notes">
              <configuration name="headers" type="delimited" locale="tl">
                <value>default:Mga tala</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="persons_present">
              <configuration name="headers" type="delimited" locale="tl">
                <value>default:Mga Tao na Naroroon</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="editAction" path="/modules/disciplinary_action/page/editAction">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="tl">
            <value>Magdagdag/Magsapanahon ng Hakbanging Pangdisiplina</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>