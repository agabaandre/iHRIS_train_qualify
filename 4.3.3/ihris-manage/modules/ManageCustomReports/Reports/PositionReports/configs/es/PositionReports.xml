<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="ihris-manage-CustomReports-position-reports" path="/modules/CustomReports">
  <metadata>
    <version>4.2.2.0.1</version>
  </metadata>
  <configurationGroup name="ihris-manage-CustomReports-position-reports" path="/modules/CustomReports" locale="es">
    <version>4.2.0.1</version>
    <configurationGroup name="relationships" config="CustomReports_Relationships">
      <configurationGroup name="position" config="CustomReports_Relationship">
        <configurationGroup name="joins">
          <configurationGroup name="job">
            <configurationGroup name="joins">
              <configurationGroup name="cadre">
                <configuration name="display_name" locale="es">
                  <value>Cuadro</value>
                </configuration>
                <configuration name="description" locale="es">
                  <value>Cuadro Asociado con este Empleo</value>
                </configuration>
              </configurationGroup>
              <configurationGroup name="classification">
                <configuration name="display_name" locale="es">
                  <value>Clasificación</value>
                </configuration>
                <configuration name="description" locale="es">
                  <value>Clasificación Asociada a este Empleo</value>
                </configuration>
              </configurationGroup>
            </configurationGroup>
            <configuration name="display_name" locale="es">
              <value>Empleo</value>
            </configuration>
            <configuration name="description" locale="es">
              <value>El trabajo asociado a este puesto</value>
            </configuration>
          </configurationGroup>
          <configurationGroup name="facility">
            <configuration name="display_name" locale="es">
              <value>Localidad</value>
            </configuration>
            <configuration name="description" locale="es">
              <value>La Localidad en la que esta Posición Trabaja</value>
            </configuration>
          </configurationGroup>
          <configurationGroup name="department">
            <configuration name="display_name" locale="es">
              <value>Departamento</value>
            </configuration>
            <configuration name="description" locale="es">
              <value>El Departamento a que Pertenece este Puesto</value>
            </configuration>
          </configurationGroup>
          <configurationGroup name="last">
            <configuration name="display_name" locale="es">
              <value>Última posición</value>
            </configuration>
            <configuration name="description" locale="es">
              <value>La última persona que ocupó esta posición.</value>
            </configuration>
          </configurationGroup>
          <configurationGroup name="current">
            <configuration name="display_name" locale="es">
              <value>Puesto Actual</value>
            </configuration>
            <configuration name="description" locale="es">
              <value>La persona que ocupa actualmente este puesto</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="reporting_functions">
          <configurationGroup name="days_open">
            <configuration name="description" locale="es">
              <value>La cantidad de días que esta posición estuvo abierta antes de se llenada</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
        <configuration name="display_name" locale="es">
          <value>Posiciones</value>
        </configuration>
        <configuration name="description" locale="es">
          <value>Todas las Informaciones relacionadas a esta Posición</value>
        </configuration>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="reports" config="CustomReports_Reports">
      <configurationGroup name="position_list" config="CustomReports_Report">
        <configurationGroup name="meta" config="CustomReports_Report_Meta">
          <configuration name="display_name" locale="es">
            <value>Listado de Posiciones</value>
          </configuration>
          <configuration name="description" locale="es">
            <value>Detalles para todas las Posiciones</value>
          </configuration>
          <configuration name="category" locale="es">
            <value>Reportes de Posiciones</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="reporting_forms" config="CustomReports_Report_ReportingForms">
          <configurationGroup name="primary_form" config="CustomReports_Report_ReportingForm">
            <configurationGroup name="fields" config="CustomReports_Report_ReportingForm_Fields">
              <configurationGroup name="code" config="CustomReports_Report_ReportingForm_Field">
                <configuration name="header" locale="es">
                  <value>Código</value>
                </configuration>
              </configurationGroup>
              <configurationGroup name="job" config="CustomReports_Report_ReportingForm_Field">
                <configuration name="header" locale="es">
                  <value>Empleo de la Posición</value>
                </configuration>
                <configurationGroup name="limits">
                  <configurationGroup name="equals">
                    <configuration name="header" locale="es">
                      <value>Empleo</value>
                    </configuration>
                  </configurationGroup>
                </configurationGroup>
              </configurationGroup>
              <configurationGroup name="title" config="CustomReports_Report_ReportingForm_Field">
                <configuration name="header" locale="es">
                  <value>Título</value>
                </configuration>
              </configurationGroup>
              <configurationGroup name="facility" config="CustomReports_Report_ReportingForm_Field">
                <configuration name="header" locale="es">
                  <value>ID del Local</value>
                </configuration>
                <configurationGroup name="limits">
                  <configurationGroup name="equals">
                    <configuration name="header" locale="es">
                      <value>Localidad</value>
                    </configuration>
                  </configurationGroup>
                </configurationGroup>
              </configurationGroup>
              <configurationGroup name="department" config="CustomReports_Report_ReportingForm_Field">
                <configuration name="header" locale="es">
                  <value>ID Departamento</value>
                </configuration>
                <configurationGroup name="limits">
                  <configurationGroup name="equals">
                    <configuration name="header" locale="es">
                      <value>Departamento</value>
                    </configuration>
                  </configurationGroup>
                </configurationGroup>
              </configurationGroup>
              <configurationGroup name="posted_date" config="CustomReports_Report_ReportingForm_Field">
                <configuration name="header" locale="es">
                  <value>Fecha de Publicación</value>
                </configuration>
              </configurationGroup>
              <configurationGroup name="status" config="CustomReports_Report_ReportingForm_Field">
                <configuration name="header" locale="es">
                  <value>Estado</value>
                </configuration>
                <configurationGroup name="limits">
                  <configurationGroup name="equals">
                    <configuration name="header" locale="es">
                      <value>Estado</value>
                    </configuration>
                  </configurationGroup>
                </configurationGroup>
              </configurationGroup>
            </configurationGroup>
          </configurationGroup>
          <configurationGroup name="job" config="CustomReports_Report_ReportingForm">
            <configurationGroup name="fields" config="CustomReports_Report_ReportingForm_Fields">
              <configurationGroup name="title" config="CustomReports_Report_ReportingForm_Field">
                <configuration name="header" locale="es">
                  <value>Título del Puesto</value>
                </configuration>
              </configurationGroup>
              <configurationGroup name="cadre" config="CustomReports_Report_ReportingForm_Field">
                <configuration name="header" locale="es">
                  <value>ID del Cuadro</value>
                </configuration>
                <configurationGroup name="limits">
                  <configurationGroup name="equals">
                    <configuration name="header" locale="es">
                      <value>Cuadro</value>
                    </configuration>
                  </configurationGroup>
                </configurationGroup>
              </configurationGroup>
              <configurationGroup name="classification" config="CustomReports_Report_ReportingForm_Field">
                <configuration name="header" locale="es">
                  <value>Id. de clasificación</value>
                </configuration>
                <configurationGroup name="limits">
                  <configurationGroup name="equals">
                    <configuration name="header" locale="es">
                      <value>Clasificación</value>
                    </configuration>
                  </configurationGroup>
                </configurationGroup>
              </configurationGroup>
            </configurationGroup>
          </configurationGroup>
          <configurationGroup name="cadre" config="CustomReports_Report_ReportingForm">
            <configurationGroup name="fields" config="CustomReports_Report_ReportingForm_Fields">
              <configurationGroup name="name" config="CustomReports_Report_ReportingForm_Field">
                <configuration name="header" locale="es">
                  <value>Cuadro</value>
                </configuration>
              </configurationGroup>
            </configurationGroup>
          </configurationGroup>
          <configurationGroup name="classification" config="CustomReports_Report_ReportingForm">
            <configurationGroup name="fields" config="CustomReports_Report_ReportingForm_Fields">
              <configurationGroup name="name" config="CustomReports_Report_ReportingForm_Field">
                <configuration name="header" locale="es">
                  <value>Clasificación</value>
                </configuration>
              </configurationGroup>
            </configurationGroup>
          </configurationGroup>
          <configurationGroup name="facility" config="CustomReports_Report_ReportingForm">
            <configurationGroup name="fields" config="CustomReports_Report_ReportingForm_Fields">
              <configurationGroup name="name" config="CustomReports_Report_ReportingForm_Field">
                <configuration name="header" locale="es">
                  <value>Localidad</value>
                </configuration>
              </configurationGroup>
            </configurationGroup>
          </configurationGroup>
          <configurationGroup name="department" config="CustomReports_Report_ReportingForm">
            <configurationGroup name="fields" config="CustomReports_Report_ReportingForm_Fields">
              <configurationGroup name="name" config="CustomReports_Report_ReportingForm_Field">
                <configuration name="header" locale="es">
                  <value>Departamento</value>
                </configuration>
              </configurationGroup>
            </configurationGroup>
          </configurationGroup>
          <configurationGroup name="last" config="CustomReports_Report_ReportingForm">
            <configurationGroup name="fields" config="CustomReports_Report_ReportingForm_Fields">
              <configurationGroup name="end_date" config="CustomReports_Report_ReportingForm_Field">
                <configuration name="header" locale="es">
                  <value>Fecha de Inicio</value>
                </configuration>
              </configurationGroup>
            </configurationGroup>
          </configurationGroup>
          <configurationGroup name="current" config="CustomReports_Report_ReportingForm">
            <configurationGroup name="fields" config="CustomReports_Report_ReportingForm_Fields">
              <configurationGroup name="start_date" config="CustomReports_Report_ReportingForm_Field">
                <configuration name="header" locale="es">
                  <value>Fecha en que se ocupó</value>
                </configuration>
              </configurationGroup>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="reporting_functions" config="CustomReports_Report_ReportingFunctions">
          <configurationGroup name="days_open" config="CustomReports_Report_ReportingFunction">
            <configuration name="header" locale="es">
              <value>Días Abierto</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="reportViews" config="CustomReports_ReportViews">
      <configurationGroup name="position_list" config="CustomReports_ReportView">
        <configuration name="display_name" locale="es">
          <value>Listado de Posiciones</value>
        </configuration>
        <configuration name="description" locale="es">
          <value>Listado de todas las Posiciones</value>
        </configuration>
      </configurationGroup>
      <configurationGroup name="position_open_duration" config="CustomReports_ReportView">
        <configuration name="display_name" locale="es">
          <value>Duración abierta de la Posición</value>
        </configuration>
        <configuration name="description" locale="es">
          <value>La longitud en días que la posición estuvo abierta antes de ser llenada.</value>
        </configuration>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>