<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="ihris-manage-CustomReports-position-reports" path="/modules/CustomReports">
  <metadata>
    <version>4.2.2.0.1</version>
  </metadata>
  <configurationGroup name="ihris-manage-CustomReports-position-reports" path="/modules/CustomReports" locale="gl">
    <version>4.2.0.1</version>
    <configurationGroup name="relationships" config="CustomReports_Relationships">
      <configurationGroup name="position" config="CustomReports_Relationship">
        <configurationGroup name="joins">
          <configurationGroup name="job">
            <configurationGroup name="joins">
              <configurationGroup name="cadre">
                <configuration name="display_name" locale="gl">
                  <value>Cadro</value>
                </configuration>
                <configuration name="description" locale="gl">
                  <value>O cadro asociado a este emprego.</value>
                </configuration>
              </configurationGroup>
              <configurationGroup name="classification">
                <configuration name="display_name" locale="gl">
                  <value>Clasificación</value>
                </configuration>
                <configuration name="description" locale="gl">
                  <value>A clasificación asociada a este emprego.</value>
                </configuration>
              </configurationGroup>
            </configurationGroup>
            <configuration name="display_name" locale="gl">
              <value>Emprego</value>
            </configuration>
            <configuration name="description" locale="gl">
              <value>O emprego asociado a este cargo.</value>
            </configuration>
          </configurationGroup>
          <configurationGroup name="facility">
            <configuration name="display_name" locale="gl">
              <value>Establecemento</value>
            </configuration>
            <configuration name="description" locale="gl">
              <value>O establecemento onde traballa este cargo.</value>
            </configuration>
          </configurationGroup>
          <configurationGroup name="department">
            <configuration name="display_name" locale="gl">
              <value>Departamento</value>
            </configuration>
            <configuration name="description" locale="gl">
              <value>O departamento ao que pertence este cargo.</value>
            </configuration>
          </configurationGroup>
          <configurationGroup name="last">
            <configuration name="display_name" locale="gl">
              <value>Último cargo</value>
            </configuration>
            <configuration name="description" locale="gl">
              <value>A última persoa que ocupou este cargo.</value>
            </configuration>
          </configurationGroup>
          <configurationGroup name="current">
            <configuration name="display_name" locale="gl">
              <value>Cargo actual</value>
            </configuration>
            <configuration name="description" locale="gl">
              <value>A persoa que ocupa actualmente este cargo.</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="reporting_functions">
          <configurationGroup name="days_open">
            <configuration name="description" locale="gl">
              <value>O número de días que estivo vacante este cargo antes de ser ocupado.</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
        <configuration name="display_name" locale="gl">
          <value>Cargos</value>
        </configuration>
        <configuration name="description" locale="gl">
          <value>Toda a información en relación a un cargo.</value>
        </configuration>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="reports" config="CustomReports_Reports">
      <configurationGroup name="position_list" config="CustomReports_Report">
        <configurationGroup name="meta" config="CustomReports_Report_Meta">
          <configuration name="display_name" locale="gl">
            <value>Lista de cargos</value>
          </configuration>
          <configuration name="description" locale="gl">
            <value>Detalles de todos os cargos.</value>
          </configuration>
          <configuration name="category" locale="gl">
            <value>Informes sobre o cargo</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="reporting_forms" config="CustomReports_Report_ReportingForms">
          <configurationGroup name="primary_form" config="CustomReports_Report_ReportingForm">
            <configurationGroup name="fields" config="CustomReports_Report_ReportingForm_Fields">
              <configurationGroup name="code" config="CustomReports_Report_ReportingForm_Field">
                <configuration name="header" locale="gl">
                  <value>Código</value>
                </configuration>
              </configurationGroup>
              <configurationGroup name="job" config="CustomReports_Report_ReportingForm_Field">
                <configuration name="header" locale="gl">
                  <value>Tarefa do cargo</value>
                </configuration>
                <configurationGroup name="limits">
                  <configurationGroup name="equals">
                    <configuration name="header" locale="gl">
                      <value>Emprego</value>
                    </configuration>
                  </configurationGroup>
                </configurationGroup>
              </configurationGroup>
              <configurationGroup name="title" config="CustomReports_Report_ReportingForm_Field">
                <configuration name="header" locale="gl">
                  <value>Título</value>
                </configuration>
              </configurationGroup>
              <configurationGroup name="facility" config="CustomReports_Report_ReportingForm_Field">
                <configuration name="header" locale="gl">
                  <value>ID do establecemento</value>
                </configuration>
                <configurationGroup name="limits">
                  <configurationGroup name="equals">
                    <configuration name="header" locale="gl">
                      <value>Establecemento</value>
                    </configuration>
                  </configurationGroup>
                </configurationGroup>
              </configurationGroup>
              <configurationGroup name="department" config="CustomReports_Report_ReportingForm_Field">
                <configuration name="header" locale="gl">
                  <value>ID do departamento</value>
                </configuration>
                <configurationGroup name="limits">
                  <configurationGroup name="equals">
                    <configuration name="header" locale="gl">
                      <value>Departamento</value>
                    </configuration>
                  </configurationGroup>
                </configurationGroup>
              </configurationGroup>
              <configurationGroup name="posted_date" config="CustomReports_Report_ReportingForm_Field">
                <configuration name="header" locale="gl">
                  <value>Data de publicación</value>
                </configuration>
              </configurationGroup>
              <configurationGroup name="status" config="CustomReports_Report_ReportingForm_Field">
                <configuration name="header" locale="gl">
                  <value>Estado</value>
                </configuration>
                <configurationGroup name="limits">
                  <configurationGroup name="equals">
                    <configuration name="header" locale="gl">
                      <value>Estado</value>
                    </configuration>
                  </configurationGroup>
                </configurationGroup>
              </configurationGroup>
            </configurationGroup>
          </configurationGroup>
          <configurationGroup name="job" config="CustomReports_Report_ReportingForm">
            <configurationGroup name="fields" config="CustomReports_Report_ReportingForm_Fields">
              <configurationGroup name="title" config="CustomReports_Report_ReportingForm_Field">
                <configuration name="header" locale="gl">
                  <value>Título do emprego</value>
                </configuration>
              </configurationGroup>
              <configurationGroup name="cadre" config="CustomReports_Report_ReportingForm_Field">
                <configuration name="header" locale="gl">
                  <value>ID do cadro</value>
                </configuration>
                <configurationGroup name="limits">
                  <configurationGroup name="equals">
                    <configuration name="header" locale="gl">
                      <value>Cadro</value>
                    </configuration>
                  </configurationGroup>
                </configurationGroup>
              </configurationGroup>
              <configurationGroup name="classification" config="CustomReports_Report_ReportingForm_Field">
                <configuration name="header" locale="gl">
                  <value>ID da clasificación</value>
                </configuration>
                <configurationGroup name="limits">
                  <configurationGroup name="equals">
                    <configuration name="header" locale="gl">
                      <value>Clasificación</value>
                    </configuration>
                  </configurationGroup>
                </configurationGroup>
              </configurationGroup>
            </configurationGroup>
          </configurationGroup>
          <configurationGroup name="cadre" config="CustomReports_Report_ReportingForm">
            <configurationGroup name="fields" config="CustomReports_Report_ReportingForm_Fields">
              <configurationGroup name="name" config="CustomReports_Report_ReportingForm_Field">
                <configuration name="header" locale="gl">
                  <value>Cadro</value>
                </configuration>
              </configurationGroup>
            </configurationGroup>
          </configurationGroup>
          <configurationGroup name="classification" config="CustomReports_Report_ReportingForm">
            <configurationGroup name="fields" config="CustomReports_Report_ReportingForm_Fields">
              <configurationGroup name="name" config="CustomReports_Report_ReportingForm_Field">
                <configuration name="header" locale="gl">
                  <value>Clasificación</value>
                </configuration>
              </configurationGroup>
            </configurationGroup>
          </configurationGroup>
          <configurationGroup name="facility" config="CustomReports_Report_ReportingForm">
            <configurationGroup name="fields" config="CustomReports_Report_ReportingForm_Fields">
              <configurationGroup name="name" config="CustomReports_Report_ReportingForm_Field">
                <configuration name="header" locale="gl">
                  <value>Establecemento</value>
                </configuration>
              </configurationGroup>
            </configurationGroup>
          </configurationGroup>
          <configurationGroup name="department" config="CustomReports_Report_ReportingForm">
            <configurationGroup name="fields" config="CustomReports_Report_ReportingForm_Fields">
              <configurationGroup name="name" config="CustomReports_Report_ReportingForm_Field">
                <configuration name="header" locale="gl">
                  <value>Departamento</value>
                </configuration>
              </configurationGroup>
            </configurationGroup>
          </configurationGroup>
          <configurationGroup name="last" config="CustomReports_Report_ReportingForm">
            <configurationGroup name="fields" config="CustomReports_Report_ReportingForm_Fields">
              <configurationGroup name="end_date" config="CustomReports_Report_ReportingForm_Field">
                <configuration name="header" locale="gl">
                  <value>Data de apertura</value>
                </configuration>
              </configurationGroup>
            </configurationGroup>
          </configurationGroup>
          <configurationGroup name="current" config="CustomReports_Report_ReportingForm">
            <configurationGroup name="fields" config="CustomReports_Report_ReportingForm_Fields">
              <configurationGroup name="start_date" config="CustomReports_Report_ReportingForm_Field">
                <configuration name="header" locale="gl">
                  <value>Data de ocupación</value>
                </configuration>
              </configurationGroup>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="reporting_functions" config="CustomReports_Report_ReportingFunctions">
          <configurationGroup name="days_open" config="CustomReports_Report_ReportingFunction">
            <configuration name="header" locale="gl">
              <value>Días vacante</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="reportViews" config="CustomReports_ReportViews">
      <configurationGroup name="position_list" config="CustomReports_ReportView">
        <configuration name="display_name" locale="gl">
          <value>Lista de cargos</value>
        </configuration>
        <configuration name="description" locale="gl">
          <value>Unha lista de todos os cargos.</value>
        </configuration>
      </configurationGroup>
      <configurationGroup name="position_open_duration" config="CustomReports_ReportView">
        <configuration name="display_name" locale="gl">
          <value>Duración da vacante no cargo</value>
        </configuration>
        <configuration name="description" locale="gl">
          <value>A duración, en días, que cada cargo estivo vacante antes de ser ocupado.</value>
        </configuration>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>