<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="ihris-manage-CustomReports-search-people" path="/modules/CustomReports">
  <metadata>
    <version>4.2.2.0.1</version>
  </metadata>
  <configurationGroup name="ihris-manage-CustomReports-search-people" path="/modules/CustomReports" locale="zh_CN">
    <version>4.2.0.1</version>
    <configurationGroup name="relationships">
      <configurationGroup name="search_people">
        <configuration name="display_name" locale="zh_CN">
          <value>搜寻人</value>
        </configuration>
        <configuration name="description" locale="zh_CN">
          <value>为所有人的记录搜索关系。</value>
        </configuration>
        <configurationGroup name="reporting_functions">
          <configurationGroup name="employee_status">
            <configuration name="description" locale="zh_CN"></configuration>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="joins">
          <configurationGroup name="last_person_position">
            <configuration name="description" locale="zh_CN"></configuration>
            <configuration name="display_name" locale="zh_CN"></configuration>
            <configurationGroup name="joins">
              <configurationGroup name="last_position">
                <configuration name="description" locale="zh_CN"></configuration>
                <configuration name="display_name" locale="zh_CN">
                  <value>最新位置</value>
                </configuration>
                <configurationGroup name="joins">
                  <configurationGroup name="last_job">
                    <configuration name="description" locale="zh_CN"></configuration>
                    <configuration name="display_name" locale="zh_CN">
                      <value>最近工作</value>
                    </configuration>
                  </configurationGroup>
                  <configurationGroup name="last_facility">
                    <configuration name="description" locale="zh_CN"></configuration>
                    <configuration name="display_name" locale="zh_CN">
                      <value>最新设施</value>
                    </configuration>
                  </configurationGroup>
                </configurationGroup>
              </configurationGroup>
            </configurationGroup>
          </configurationGroup>
          <configurationGroup name="application">
            <configuration name="description" locale="zh_CN"></configuration>
            <configuration name="display_name" locale="zh_CN">
              <value>申请</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="reports">
      <configurationGroup name="search_people">
        <configurationGroup name="meta">
          <configuration name="display_name" locale="zh_CN">
            <value>搜寻人</value>
          </configuration>
          <configuration name="description" locale="zh_CN">
            <value>查询所有人的记录</value>
          </configuration>
          <configuration name="category" locale="zh_CN">
            <value>搜索</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="reporting_forms">
          <configurationGroup name="primary_form">
            <configurationGroup name="fields">
              <configurationGroup name="surname">
                <configurationGroup name="limits">
                  <configurationGroup name="contains">
                    <configuration name="header" locale="zh_CN">
                      <value>姓名</value>
                    </configuration>
                  </configurationGroup>
                </configurationGroup>
                <configuration name="header" locale="zh_CN">
                  <value>姓名</value>
                </configuration>
              </configurationGroup>
              <configurationGroup name="firstname">
                <configuration name="header" locale="zh_CN">
                  <value>名</value>
                </configuration>
              </configurationGroup>
            </configurationGroup>
          </configurationGroup>
          <configurationGroup name="last_person_position">
            <configurationGroup name="fields">
              <configurationGroup name="start_date">
                <configuration name="header" locale="zh_CN">
                  <value>开始日期</value>
                </configuration>
              </configurationGroup>
              <configurationGroup name="end_date">
                <configuration name="header" locale="zh_CN">
                  <value>结束日期</value>
                </configuration>
              </configurationGroup>
            </configurationGroup>
          </configurationGroup>
          <configurationGroup name="last_position">
            <configurationGroup name="fields">
              <configurationGroup name="job">
                <configurationGroup name="limits">
                  <configurationGroup name="equals">
                    <configuration name="header" locale="zh_CN">
                      <value>工作</value>
                    </configuration>
                  </configurationGroup>
                </configurationGroup>
                <configuration name="header" locale="zh_CN"></configuration>
              </configurationGroup>
              <configurationGroup name="title">
                <configuration name="header" locale="zh_CN">
                  <value>职业标题</value>
                </configuration>
              </configurationGroup>
              <configurationGroup name="facility">
                <configurationGroup name="limits">
                  <configurationGroup name="equals">
                    <configuration name="header" locale="zh_CN">
                      <value>设施</value>
                    </configuration>
                  </configurationGroup>
                </configurationGroup>
                <configuration name="header" locale="zh_CN">
                  <value>设施 ID</value>
                </configuration>
              </configurationGroup>
            </configurationGroup>
          </configurationGroup>
          <configurationGroup name="last_facility">
            <configurationGroup name="fields">
              <configurationGroup name="name">
                <configuration name="header" locale="zh_CN">
                  <value>设施</value>
                </configuration>
              </configurationGroup>
            </configurationGroup>
          </configurationGroup>
          <configurationGroup name="last_job">
            <configurationGroup name="fields">
              <configurationGroup name="title">
                <configuration name="header" locale="zh_CN">
                  <value>职业名称</value>
                </configuration>
              </configurationGroup>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="reporting_functions">
          <configurationGroup name="employee_status">
            <configurationGroup name="limits">
              <configurationGroup name="equals">
                <configuration name="header" locale="zh_CN">
                  <value>员工状态</value>
                </configuration>
              </configurationGroup>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="reportViews">
      <configurationGroup name="search_people">
        <configuration name="description" locale="zh_CN"></configuration>
        <configuration name="display_name" locale="zh_CN">
          <value>搜寻人</value>
        </configuration>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>