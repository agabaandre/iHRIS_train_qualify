<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="ihris-manage-CustomReports-search-people" path="/modules/CustomReports">
  <metadata>
    <version>4.2.2.0.1</version>
  </metadata>
  <configurationGroup name="ihris-manage-CustomReports-search-people" path="/modules/CustomReports" locale="nl">
    <version>4.2.0.1</version>
    <configurationGroup name="relationships">
      <configurationGroup name="search_people">
        <configuration name="display_name" locale="nl">
          <value>Mensen zoeken</value>
        </configuration>
        <configuration name="description" locale="nl">
