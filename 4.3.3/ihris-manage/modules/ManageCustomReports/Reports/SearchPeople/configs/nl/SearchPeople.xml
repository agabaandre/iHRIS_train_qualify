        </configuration>
        <configurationGroup name="reporting_functions">
          <configurationGroup name="employee_status">
            <configuration name="description" locale="nl">
              <value>De huidige status van deze medewerker</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="joins">
          <configurationGroup name="last_person_position">
            <configuration name="description" locale="nl">
              <value>De vorige positie die deze persoon heeft vervuld.</value>
            </configuration>
            <configuration name="display_name" locale="nl">
              <value>Vorige positiepersoon</value>
            </configuration>
            <configurationGroup name="joins">
              <configurationGroup name="last_position">
                <configuration name="description" locale="nl">
                  <value>Gegevens over de vorige positie die deze persoon heeft vervuld.</value>
                </configuration>
                <configuration name="display_name" locale="nl">
                  <value>Vorige positie</value>
                </configuration>
                <configurationGroup name="joins">
                  <configurationGroup name="last_job">
                    <configuration name="description" locale="nl">
                      <value>De laatste functie die door deze persoon is vervuld.</value>
                    </configuration>
                    <configuration name="display_name" locale="nl">
                      <value>Laatste functie</value>
                    </configuration>
                  </configurationGroup>
                  <configurationGroup name="last_facility">
                    <configuration name="description" locale="nl">
                      <value>De vorige instelling waar deze persoon heeft gewerkt.</value>
                    </configuration>
                    <configuration name="display_name" locale="nl">
                      <value>Vorige instelling</value>
                    </configuration>
                  </configurationGroup>
                </configurationGroup>
              </configurationGroup>
            </configurationGroup>
          </configurationGroup>
          <configurationGroup name="application">
            <configuration name="description" locale="nl">
              <value>Details over de sollicitatie van deze persoon.</value>
            </configuration>
            <configuration name="display_name" locale="nl">
              <value>Sollicitatie</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="reports">
      <configurationGroup name="search_people">
        <configurationGroup name="meta">
          <configuration name="display_name" locale="nl">
            <value>Mensen zoeken</value>
          </configuration>
          <configuration name="description" locale="nl">
            <value>Dossiers van alle personen doorzoeken.</value>
