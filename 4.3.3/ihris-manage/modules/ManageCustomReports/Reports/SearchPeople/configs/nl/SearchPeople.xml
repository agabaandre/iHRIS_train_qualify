              </configurationGroup>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="reporting_functions">
          <configurationGroup name="employee_status">
            <configurationGroup name="limits">
              <configurationGroup name="equals">
                <configuration name="header" locale="nl">
                  <value>Status medewerker</value>
                </configuration>
              </configurationGroup>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="reportViews">
      <configurationGroup name="search_people">
        <configuration name="description" locale="nl">
          <value>Alle persoonsdossiers in het systeem doorzoeken.</value>
        </configuration>
        <configuration name="display_name" locale="nl">
          <value>Mensen zoeken</value>
        </configuration>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>