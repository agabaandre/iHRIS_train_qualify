        <configurationGroup name="reporting_functions">
          <configurationGroup name="employee_status">
            <configurationGroup name="limits">
              <configurationGroup name="equals">
                <configuration name="header" locale="es">
                  <value>Estado del empleado</value>
                </configuration>
              </configurationGroup>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="reportViews">
      <configurationGroup name="search_people">
        <configuration name="description" locale="es">
          <value>Buscar todos los registros de esta Persona en el Sistema</value>
        </configuration>
        <configuration name="display_name" locale="es">
          <value>Buscar Personas</value>
        </configuration>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>