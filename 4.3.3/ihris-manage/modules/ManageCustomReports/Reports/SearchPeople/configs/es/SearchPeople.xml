            </configuration>
            <configuration name="display_name" locale="es">
              <value>Solicitud</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="reports">
      <configurationGroup name="search_people">
        <configurationGroup name="meta">
          <configuration name="display_name" locale="es">
            <value>Buscar Personas</value>
