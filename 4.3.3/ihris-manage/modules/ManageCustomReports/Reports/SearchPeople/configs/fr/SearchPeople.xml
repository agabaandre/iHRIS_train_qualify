<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="ihris-manage-CustomReports-search-people" path="/modules/CustomReports">
  <metadata>
    <version>4.2.2.0.1</version>
  </metadata>
  <configurationGroup name="ihris-manage-CustomReports-search-people" path="/modules/CustomReports" locale="fr">
    <version>4.2.0.1</version>
    <configurationGroup name="relationships">
      <configurationGroup name="search_people">
        <configuration name="display_name" locale="fr">
          <value>Rechercher des personnes</value>
        </configuration>
        <configuration name="description" locale="fr">
          <value>Rechercher un lien pour tous les enregistrements de personnes.</value>
        </configuration>
        <configurationGroup name="reporting_functions">
          <configurationGroup name="employee_status">
            <configuration name="description" locale="fr">
              <value>Le statut actuel de cet employé</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="joins">
          <configurationGroup name="last_person_position">
            <configuration name="description" locale="fr">
              <value>Le dernier poste que cette personne a occupé.</value>
            </configuration>
            <configuration name="display_name" locale="fr">
              <value>Dernier poste occupé par la personne</value>
            </configuration>
            <configurationGroup name="joins">
              <configurationGroup name="last_position">
                <configuration name="description" locale="fr">
                  <value>Renseignements de poste pour le dernier poste occupé par cette personne.</value>
                </configuration>
                <configuration name="display_name" locale="fr">
                  <value>Dernier poste</value>
                </configuration>
                <configurationGroup name="joins">
                  <configurationGroup name="last_job">
                    <configuration name="description" locale="fr">
                      <value>Le dernier emploi occupé par cette personne.</value>
                    </configuration>
                    <configuration name="display_name" locale="fr">
                      <value>Dernier emploi</value>
                    </configuration>
                  </configurationGroup>
                  <configurationGroup name="last_facility">
                    <configuration name="description" locale="fr">
                      <value>Le dernier établissement où cette personne a travaillé.</value>
                    </configuration>
                    <configuration name="display_name" locale="fr">
                      <value>Dernier établissement</value>
                    </configuration>
                  </configurationGroup>
                </configurationGroup>
              </configurationGroup>
            </configurationGroup>
          </configurationGroup>
          <configurationGroup name="application">
            <configuration name="description" locale="fr">
              <value>Précisions sur la candidature de cette personne.</value>
            </configuration>
            <configuration name="display_name" locale="fr">
              <value>Candidature</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="reports">
      <configurationGroup name="search_people">
        <configurationGroup name="meta">
          <configuration name="display_name" locale="fr">
            <value>Rechercher des personnes</value>
          </configuration>
          <configuration name="description" locale="fr">
            <value>Rechercher tous les enregistrements de personnes.</value>
          </configuration>
          <configuration name="category" locale="fr">
            <value>Recherche</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="reporting_forms">
          <configurationGroup name="primary_form">
            <configurationGroup name="fields">
              <configurationGroup name="surname">
                <configurationGroup name="limits">
                  <configurationGroup name="contains">
                    <configuration name="header" locale="fr">
                      <value>Nom de famille</value>
                    </configuration>
                  </configurationGroup>
                </configurationGroup>
                <configuration name="header" locale="fr">
                  <value>Nom de famille</value>
                </configuration>
              </configurationGroup>
              <configurationGroup name="firstname">
                <configuration name="header" locale="fr">
                  <value>Prénom</value>
                </configuration>
              </configurationGroup>
            </configurationGroup>
          </configurationGroup>
          <configurationGroup name="last_person_position">
            <configurationGroup name="fields">
              <configurationGroup name="start_date">
                <configuration name="header" locale="fr">
                  <value>Date de début</value>
                </configuration>
              </configurationGroup>
              <configurationGroup name="end_date">
                <configuration name="header" locale="fr">
                  <value>Date de fin</value>
                </configuration>
              </configurationGroup>
            </configurationGroup>
          </configurationGroup>
          <configurationGroup name="last_position">
            <configurationGroup name="fields">
              <configurationGroup name="job">
                <configurationGroup name="limits">
                  <configurationGroup name="equals">
                    <configuration name="header" locale="fr">
                      <value>Emploi</value>
                    </configuration>
                  </configurationGroup>
                </configurationGroup>
                <configuration name="header" locale="fr">
                  <value>ID du Poste Emploi</value>
                </configuration>
              </configurationGroup>
              <configurationGroup name="title">
                <configuration name="header" locale="fr">
                  <value>Intitulé du poste</value>
                </configuration>
              </configurationGroup>
              <configurationGroup name="facility">
                <configurationGroup name="limits">
                  <configurationGroup name="equals">
                    <configuration name="header" locale="fr">
                      <value>Établissement</value>
                    </configuration>
                  </configurationGroup>
                </configurationGroup>
                <configuration name="header" locale="fr">
                  <value>ID de l’Etablissement</value>
                </configuration>
              </configurationGroup>
            </configurationGroup>
          </configurationGroup>
          <configurationGroup name="last_facility">
            <configurationGroup name="fields">
              <configurationGroup name="name">
                <configuration name="header" locale="fr">
                  <value>Établissement</value>
                </configuration>
              </configurationGroup>
            </configurationGroup>
          </configurationGroup>
          <configurationGroup name="last_job">
            <configurationGroup name="fields">
              <configurationGroup name="title">
                <configuration name="header" locale="fr">
                  <value>Intitulé de l'emploi</value>
                </configuration>
              </configurationGroup>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="reporting_functions">
          <configurationGroup name="employee_status">
            <configurationGroup name="limits">
              <configurationGroup name="equals">
                <configuration name="header" locale="fr">
                  <value>Statut de l’employé</value>
                </configuration>
              </configurationGroup>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="reportViews">
      <configurationGroup name="search_people">
        <configuration name="description" locale="fr">
          <value>Rechercher tous les enregistrements de personnes dans le système.</value>
        </configuration>
        <configuration name="display_name" locale="fr">
          <value>Rechercher des personnes</value>
        </configuration>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>