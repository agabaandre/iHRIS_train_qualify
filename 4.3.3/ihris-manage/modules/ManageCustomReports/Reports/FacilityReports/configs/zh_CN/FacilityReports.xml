<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="ihris-manage-CustomReports-facility-reports" path="/modules/CustomReports">
  <metadata>
    <version>4.2.2.0.1</version>
  </metadata>
  <configurationGroup name="ihris-manage-CustomReports-facility-reports" path="/modules/CustomReports" locale="zh_CN">
    <version>4.2.0.1</version>
    <configurationGroup name="relationships">
      <configurationGroup name="facility">
        <configuration name="display_name" locale="zh_CN">
          <value>设备列表</value>
        </configuration>
        <configuration name="description" locale="zh_CN">
          <value>设施信息</value>
        </configuration>
        <configurationGroup name="joins">
          <configurationGroup name="facility_type">
            <configuration name="description" locale="zh_CN">
              <value>这项设施的设备类型。</value>
            </configuration>
            <configuration name="display_name" locale="zh_CN">
              <value>设备类型</value>
            </configuration>
          </configurationGroup>
          <configurationGroup name="facility_contact">
            <configuration name="description" locale="zh_CN">
              <value>这项设施的联系信息。</value>
            </configuration>
            <configuration name="display_name" locale="zh_CN">
              <value>设施联系</value>
            </configuration>
          </configurationGroup>
          <configurationGroup name="district">
            <configuration name="description" locale="zh_CN">
              <value>这项设施在哪里区。</value>
            </configuration>
            <configuration name="display_name" locale="zh_CN">
              <value>区</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="reports">
      <configurationGroup name="facility_list">
        <configurationGroup name="meta">
          <configuration name="display_name" locale="zh_CN">
            <value>设备列表</value>
          </configuration>
          <configuration name="description" locale="zh_CN">
            <value>设备的信息。</value>
          </configuration>
          <configuration name="category" locale="zh_CN">
            <value>基金报告</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="reporting_forms">
          <configurationGroup name="primary_form">
            <configurationGroup name="fields">
              <configurationGroup name="facility_type">
                <configurationGroup name="limits">
                  <configurationGroup name="equals">
                    <configuration name="header" locale="zh_CN">
                      <value>设备类型</value>
                    </configuration>
                  </configurationGroup>
                </configurationGroup>
                <configuration name="header" locale="zh_CN">
                  <value>基金类型 ID</value>
                </configuration>
              </configurationGroup>
              <configurationGroup name="location">
                <configurationGroup name="limits">
                  <configurationGroup name="equals">
                    <configuration name="header" locale="zh_CN">
                      <value>设施选址</value>
                    </configuration>
                  </configurationGroup>
                  <configurationGroup name="within">
                    <configuration name="header" locale="zh_CN">
                      <value>设施选址</value>
                    </configuration>
                  </configurationGroup>
                </configurationGroup>
                <configuration name="header" locale="zh_CN">
                  <value>设施位置 ID</value>
                </configuration>
              </configurationGroup>
              <configurationGroup name="name">
                <configuration name="header" locale="zh_CN">
                  <value>设备名称</value>
                </configuration>
              </configurationGroup>
            </configurationGroup>
          </configurationGroup>
          <configurationGroup name="district">
            <configurationGroup name="fields">
              <configurationGroup name="name">
                <configuration name="header" locale="zh_CN">
                  <value>位置</value>
                </configuration>
              </configurationGroup>
            </configurationGroup>
          </configurationGroup>
          <configurationGroup name="facility_contact">
            <configurationGroup name="fields">
              <configurationGroup name="address">
                <configuration name="header" locale="zh_CN">
                  <value>邮寄地址</value>
                </configuration>
              </configurationGroup>
              <configurationGroup name="telephone">
                <configuration name="header" locale="zh_CN">
                  <value>电话号码</value>
                </configuration>
              </configurationGroup>
              <configurationGroup name="email">
                <configuration name="header" locale="zh_CN">
                  <value>电子邮件地址</value>
                </configuration>
              </configurationGroup>
              <configurationGroup name="notes">
                <configuration name="header" locale="zh_CN">
                  <value>联系人说明</value>
                </configuration>
              </configurationGroup>
            </configurationGroup>
          </configurationGroup>
          <configurationGroup name="facility_type">
            <configurationGroup name="fields">
              <configurationGroup name="name">
                <configuration name="header" locale="zh_CN">
                  <value>设备类型</value>
                </configuration>
              </configurationGroup>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="reportViews">
      <configurationGroup name="facility_list">
        <configuration name="description" locale="zh_CN">
          <value>所有设备的列表。</value>
        </configuration>
        <configuration name="display_name" locale="zh_CN">
          <value>设备列表</value>
        </configuration>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>