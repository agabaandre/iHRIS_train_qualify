<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="ManageSelfService" path="/">
  <metadata>
    <version>4.2.2.0.1</version>
  </metadata>
  <configurationGroup name="ManageSelfService" path="/" locale="pt_BR">
    <version>4.2.0.1</version>
    <configurationGroup name="SS" path="/modules/SelfService">
      <configurationGroup name="page">
        <configurationGroup name="apply">
          <configurationGroup name="args">
            <configuration name="title" values="single" locale="pt_BR">
              <value>Adicionar/Atualizar Pessoa</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="open_position_reports" path="/modules/CustomReports">
      <configurationGroup name="relationships">
        <configurationGroup name="open_position" config="CustomReports_Relationship">
          <configuration name="display_name" locale="pt_BR">
            <value>Cargos Abertos</value>
          </configuration>
          <configuration name="description" locale="pt_BR">
            <value>Cargos Abertos</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="reports" config="CustomReports_Reports">
        <configurationGroup name="open_position_list" config="CustomReports_Report">
          <configurationGroup name="meta" config="CustomReports_Report_Meta">
            <configuration name="display_name" locale="pt_BR">
              <value>Lista de Cargos</value>
            </configuration>
            <configuration name="description" locale="pt_BR">
              <value>Detalhes de todos os cargos.</value>
            </configuration>
            <configuration name="category" locale="pt_BR">
              <value>Informações do Cargo</value>
            </configuration>
          </configurationGroup>
          <configurationGroup name="reporting_forms" config="CustomReports_Report_ReportingForms">
            <configurationGroup name="primary_form" config="CustomReports_Report_ReportingForm">
              <configurationGroup name="fields" config="CustomReports_Report_ReportingForm_Fields">
                <configurationGroup name="code" config="CustomReports_Report_ReportingForm_Field">
                  <configuration name="header" locale="pt_BR">
                    <value>Código</value>
                  </configuration>
                </configurationGroup>
                <configurationGroup name="job" config="CustomReports_Report_ReportingForm_Field">
                  <configuration name="header" locale="pt_BR">
                    <value>Cargo de Emprego</value>
                  </configuration>
                  <configurationGroup name="limits">
                    <configurationGroup name="equals">
                      <configuration name="header" locale="pt_BR">
                        <value>Trabalho</value>
                      </configuration>
                    </configurationGroup>
                  </configurationGroup>
                </configurationGroup>
                <configurationGroup name="title" config="CustomReports_Report_ReportingForm_Field">
                  <configuration name="header" locale="pt_BR">
                    <value>Título</value>
                  </configuration>
                </configurationGroup>
                <configurationGroup name="facility" config="CustomReports_Report_ReportingForm_Field">
                  <configuration name="header" locale="pt_BR">
                    <value>Identificação do Escritório</value>
                  </configuration>
                  <configurationGroup name="limits">
                    <configurationGroup name="equals">
                      <configuration name="header" locale="pt_BR">
                        <value>Escritório</value>
                      </configuration>
                    </configurationGroup>
                  </configurationGroup>
                </configurationGroup>
                <configurationGroup name="department" config="CustomReports_Report_ReportingForm_Field">
                  <configuration name="header" locale="pt_BR">
                    <value>Identificação do Departamento</value>
                  </configuration>
                  <configurationGroup name="limits">
                    <configurationGroup name="equals">
                      <configuration name="header" locale="pt_BR">
                        <value>Departamento</value>
                      </configuration>
                    </configurationGroup>
                  </configurationGroup>
                </configurationGroup>
                <configurationGroup name="posted_date" config="CustomReports_Report_ReportingForm_Field">
                  <configuration name="header" locale="pt_BR">
                    <value>Data de Postagem</value>
                  </configuration>
                </configurationGroup>
              </configurationGroup>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="reportViews" config="CustomReports_ReportViews">
        <configurationGroup name="open_position" config="CustomReports_ReportView">
          <configuration name="display_name" locale="pt_BR">
            <value>Cargos Abertos</value>
          </configuration>
          <configuration name="description" locale="pt_BR">
            <value>Os cargos atualmente abertos</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>