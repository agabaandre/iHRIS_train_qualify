<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="ihris-manage-Application" path="/I2CE">
  <metadata>
    <version>4.3.0.1</version>
  </metadata>
  <configurationGroup name="ihris-manage-Application" path="/I2CE" locale="pt">
    <version>4.3.0.1</version>
    <configurationGroup name="invalid_formfield_message" path="/modules/forms/formfield_messages" locale="pt">
      <configuration name="app_felon" path="application/felony_circumstances/required"></configuration>
      <configuration name="app_hours" path="application/hours/required"></configuration>
    </configurationGroup>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="pt">
      <configuration name="person_can_view_child_form_position_decision" locale="pt">
        <value>Pode visualizar o child form da decisão sobre o cargo de uma pessoa</value>
      </configuration>
      <configuration name="person_can_edit_child_form_position_decision" locale="pt">
        <value>Pode editar o child form da decisão sobre o cargo de uma pessoa</value>
      </configuration>
      <configuration name="person_can_view_child_form_position_interview" locale="pt">
        <value>Pode visualizar o child form de entrevista de uma pessoa</value>
      </configuration>
      <configuration name="person_can_edit_child_form_position_interview" locale="pt">
        <value>Pode editar o child form de entrevista de uma pessoa</value>
      </configuration>
      <configuration name="person_can_view_child_form_application" locale="pt"></configuration>
      <configuration name="person_can_edit_child_form_application" locale="pt"></configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="application">
          <configuration name="display" values="single" locale="pt">
            <value>Candidato</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="position_interview">
          <configuration name="display" values="single" locale="pt">
            <value>Entrevista para o Cargo</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="position_decision">
          <configuration name="display" values="single" locale="pt">
            <value>Decisão sobre o Cargo</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_Applicant">
          <configurationGroup name="fields">
            <configurationGroup name="position">
              <configuration name="headers" type="delimited" locale="pt">
                <value>default:Cargo(s)</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="felony">
              <configuration name="headers" type="delimited" locale="pt">
                <value>default:Alguma vez foi condenado por algum crime?</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="felony_circumstance">
              <configuration name="headers" type="delimited" locale="pt">
                <value>default:Se sim, indique as circunstâncias.</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="other_info">
              <configuration name="headers" type="delimited" locale="pt">
                <value>default:Adicionalmente à sua história de trabalho, possui algumas outras capacidades, qualificações e experiência que deverão ser consideradas?</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="hear">
              <configuration name="headers" type="delimited" locale="pt">
                <value>default:Como soube desta vaga?</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="start_date">
              <configuration name="headers" type="delimited" locale="pt">
                <value>default:Qual é a sua disponibilidade para começar?</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="desired_wage">
              <configuration name="headers" type="delimited" locale="pt">
                <value>default:Expectativa Salarial</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="full_time">
              <configuration name="headers" type="delimited" locale="pt">
                <value>default:Está a procura de um emprego a tempo inteiro?</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="hours">
              <configuration name="headers" type="delimited" locale="pt">
                <value>default:Se não, qual é a sua disponibilidade?</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="iHRIS_PositionInterview">
          <configurationGroup name="fields">
            <configurationGroup name="date">
              <configuration name="headers" type="delimited" locale="pt">
                <value>default:Data da entrevista</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="person">
              <configuration name="headers" type="delimited" locale="pt">
                <value>default:Pessoas que participaram</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="comments">
              <configuration name="headers" type="delimited" locale="pt">
                <value>default:Comentários</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="iHRIS_PositionDecision">
          <configurationGroup name="fields">
            <configurationGroup name="date">
              <configuration name="headers" type="delimited" locale="pt">
                <value>default:Data da Decisão</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="offer">
              <configuration name="headers" type="delimited" locale="pt">
                <value>default:Oferecer o Emprego?</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="comments">
              <configuration name="headers" type="delimited" locale="pt">
                <value>default:Comentários</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="application">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="pt">
            <value>Adicionar/Atualizar Candidatura</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="position_decision">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="pt">
            <value>Adicionar/Atualizar Decisão sobre o Cargo</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="position_interview">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="pt">
            <value>Adicionar/Atualizar Entrevista.</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>