<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="ihris-manage-Application" path="/I2CE">
  <metadata>
    <version>4.3.0.1</version>
  </metadata>
  <configurationGroup name="ihris-manage-Application" path="/I2CE" locale="nl">
    <version>4.3.0.1</version>
    <configurationGroup name="invalid_formfield_message" path="/modules/forms/formfield_messages" locale="nl">
      <configuration name="app_felon" path="application/felony_circumstances/required">
        <value>U hebt opgegeven dat u bent veroordeeld voor een misdrijf, maar u hebt de omstandigheden niet beschreven.</value>
      </configuration>
      <configuration name="app_hours" path="application/hours/required">
        <value>U hebt niet voltijds opgegeven, maar u hebt niet opgegeven hoeveel uur u beschikbaar bent.</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="nl">
      <configuration name="person_can_view_child_form_position_decision" locale="nl">
        <value>Kan positiebeslissingskindformulier van een persoon bekijken</value>
      </configuration>
      <configuration name="person_can_edit_child_form_position_decision" locale="nl">
        <value>Kan positiebeslissingskindformulier van een persoon bewerken</value>
      </configuration>
      <configuration name="person_can_view_child_form_position_interview" locale="nl">
        <value>Kan positiegesprekkindformulier van een persoon bekijken</value>
      </configuration>
      <configuration name="person_can_edit_child_form_position_interview" locale="nl">
        <value>Kan positiegesprekkindformulier van een persoon bewerken</value>
      </configuration>
      <configuration name="person_can_view_child_form_application" locale="nl">
        <value>Kan sollicitatiekindformulier van een persoon bekijken</value>
      </configuration>
      <configuration name="person_can_edit_child_form_application" locale="nl">
        <value>Kan sollicitatiekindformulier van een persoon bewerken</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="application">
          <configuration name="display" values="single" locale="nl">
            <value>Kandidaat</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="position_interview">
          <configuration name="display" values="single" locale="nl">
            <value>Positieinterview</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="position_decision">
          <configuration name="display" values="single" locale="nl">
            <value>Positiebesluit</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_Applicant">
          <configurationGroup name="fields">
            <configurationGroup name="position">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:Positie(s)</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="felony">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:Bent u ooit veroordeeld voor een misdrijf?</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="felony_circumstance">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:Zo ja, beschrijf de omstandigheden.</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="other_info">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:Zijn er in aanvulling op uw werkverleden andere vaardigheden, kwalificaties of ervaring die van belang zijn?</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="hear">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:Hoe hebt u gehoord van deze vacature?</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="start_date">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:Wanneer kunt u beginnen?</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="desired_wage">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:Gewenst salaris</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="full_time">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:Bent u op zoek naar een fulltime dienstverband?</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="hours">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:Zo nee, hoeveel uur bent u beschikbaar?</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="iHRIS_PositionInterview">
          <configurationGroup name="fields">
            <configurationGroup name="date">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:Gespreksdatum</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="person">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:Aanwezige personen</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="comments">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:Opmerkingen</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="iHRIS_PositionDecision">
          <configurationGroup name="fields">
            <configurationGroup name="date">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:Besluitdatum</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="offer">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:Een functieaanbod doen?</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="comments">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:Opmerkingen</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="application">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="nl">
            <value>Sollicitatie toevoegen of bijwerken</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="position_decision">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="nl">
            <value>Positiebesluit toevoegen of bijwerken</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="position_interview">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="nl">
            <value>Positiegesprek toevoegen of bijwerken</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>