<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="ihris-manage-Confirmation" path="/I2CE">
  <metadata>
    <version>4.3.0.1</version>
  </metadata>
  <configurationGroup name="ihris-manage-Confirmation" path="/I2CE" locale="gl">
    <version>4.3.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="gl">
      <configuration name="person_can_view_child_form_confirmation" locale="gl">
        <value>Pode ollar a ficha de confirmación dunha persoa</value>
      </configuration>
      <configuration name="person_can_edit_child_form_confirmation" locale="gl">
        <value>Pode editar as fichas de confirmación dunha persoa</value>
      </configuration>
      <configuration name="can_edit_database_list_confirmation_type" locale="gl">
        <value>Pode editar a lista da base de datos do tipo de confirmación</value>
      </configuration>
      <configuration name="can_view_database_list_confirmation_type" locale="gl">
        <value>Pode ollar a lista da base de datos do tipo de confirmación</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="confirmation">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="gl">
            <value>Engadir/Actualizar a confirmación</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="confirmation">
          <configuration name="display" values="single" locale="gl">
            <value>Confirmación</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="confirmation_type">
          <configuration name="display" values="single" locale="gl">
            <value>Tipo de confirmación</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_ConfirmationType">
          <configurationGroup name="fields">
            <configurationGroup name="probation_period">
              <configuration name="headers" type="delimited" locale="gl">
                <value>default:Período de probas (meses)</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="job">
              <configuration name="headers" type="delimited" locale="gl">
                <value>default:Emprego asociado</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="name">
              <configuration name="headers" type="delimited" locale="gl">
                <value>default:Nome</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="iHRIS_Confirmation">
          <configurationGroup name="fields">
            <configurationGroup name="valid">
              <configuration name="headers" type="delimited" locale="gl">
                <value>default:Válido</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="confirmation_type">
              <configuration name="headers" type="delimited" locale="gl">
                <value>default:Tipo de confirmación</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="date">
              <configuration name="headers" type="delimited" locale="gl">
                <value>default:Data</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>