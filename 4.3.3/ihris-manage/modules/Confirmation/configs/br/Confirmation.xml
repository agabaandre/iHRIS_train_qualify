  <configurationGroup name="ihris-manage-Confirmation" path="/I2CE" locale="br">
    <version>4.2.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="br">
      <configuration name="person_can_view_child_form_confirmation" locale="br">
        <value>A c'hall diskwel furmskridoù bugel kadarnaat un den</value>
      </configuration>
      <configuration name="person_can_edit_child_form_confirmation" locale="br">
        <value>A c'hall kemmañ furmskridoù kadarnaat un den</value>
      </configuration>
