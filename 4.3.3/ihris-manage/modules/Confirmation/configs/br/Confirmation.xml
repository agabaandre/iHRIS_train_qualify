        <configurationGroup name="confirmation">
          <configuration name="display" values="single" locale="br">
            <value>Kadarnadur</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="confirmation_type">
          <configuration name="display" values="single" locale="br">
            <value>Seurt kadarnaat</value>
