            </configurationGroup>
            <configurationGroup name="job">
              <configuration name="headers" type="delimited" locale="br"></configuration>
            </configurationGroup>
            <configurationGroup name="name">
              <configuration name="headers" type="delimited" locale="br">
                <value>default:Anv</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="iHRIS_Confirmation">
          <configurationGroup name="fields">
            <configurationGroup name="valid">
              <configuration name="headers" type="delimited" locale="br">
                <value>default:Kadarnaat</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="confirmation_type">
              <configuration name="headers" type="delimited" locale="br">
                <value>default:Seurt kadarnaat</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="date">
              <configuration name="headers" type="delimited" locale="br">
                <value>default:Deiziad</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>