    <requirement name="Uganda-standard-data-currency" />
    <requirement name="Uganda-standard-data-marital_status" />
    <requirement name="Uganda-standard-data-salary_source" />
    <requirement name="Uganda-standard-data-salary_grade" />
    <requirement name="Uganda-standard-data-benefit_type" />
    <requirement name="Uganda-standard-data-disciplinary_action_type" />
    <requirement name="Uganda-standard-data-id_type" />
    <requirement name="Uganda-standard-data-facility_type" />
    <requirement name="Uganda-standard-data-edu_type" />
    <requirement name="Uganda-standard-data-pos_change_reason" />
