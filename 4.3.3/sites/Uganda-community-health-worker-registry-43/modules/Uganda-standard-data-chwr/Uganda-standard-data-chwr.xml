    <requirement name="Uganda-standard-data-council" />-->
    <requirement name="Uganda-standard-data-language" />
    <!--<requirement name="Uganda-standard-data-facility" />
    <requirement name="Uganda-standard-data-cadre" />-->
    <requirement name="Uganda-standard-data-employment_terms" />
    <!--<requirement name="Uganda-standard-data-job" />-->
    <requirement name="Uganda-standard-data-function" />
    <requirement name="Uganda-standard-data-incentives" />
    <requirement name="Uganda-standard-data-org_support" />
    <requirement name="Uganda-standard-data-recruit_mech" />
    <requirement name="Uganda-standard-data-service" />
    <requirement name="Uganda-standard-data-parish" />
    <requirement name="Uganda-standard-data-village" />

    <priority>350</priority>
  </metadata>
  <configurationGroup name="Uganda-standard-data-chwr" path="/I2CE">
    <displayName>iHRIS Uganda Data for CHWR</displayName>
  </configurationGroup>
</I2CEConfiguration>
