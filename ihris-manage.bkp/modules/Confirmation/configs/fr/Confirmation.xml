<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="ihris-manage-Confirmation" path="/I2CE">
  <metadata>
    <version>4.3.0.1</version>
  </metadata>
  <configurationGroup name="ihris-manage-Confirmation" path="/I2CE" locale="fr">
    <version>4.3.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="fr">
      <configuration name="person_can_view_child_form_confirmation" locale="fr">
        <value>Peut voir la formulaire enfant de confirmation d'une personne</value>
      </configuration>
      <configuration name="person_can_edit_child_form_confirmation" locale="fr">
        <value>Peut éditer la formulaire enfant de conformation d'une personne</value>
      </configuration>
      <configuration name="can_edit_database_list_confirmation_type" locale="fr">
        <value>Peut éditer la liste de type de confirmation</value>
      </configuration>
      <configuration name="can_view_database_list_confirmation_type" locale="fr">
        <value>Peut voir la liste de type de confirmation</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="confirmation">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="fr">
            <value>Ajouter/Mettre à jour la confirmation</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="confirmation">
          <configuration name="display" values="single" locale="fr"></configuration>
        </configurationGroup>
        <configurationGroup name="confirmation_type">
          <configuration name="display" values="single" locale="fr">
            <value>Type de confirmation</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_ConfirmationType">
          <configurationGroup name="fields">
            <configurationGroup name="probation_period">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:La période probatoire (mois)</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="job">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:Emploi associé</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="name">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:Nom</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="iHRIS_Confirmation">
          <configurationGroup name="fields">
            <configurationGroup name="valid">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:Valable</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="confirmation_type">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:Type de confirmation</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="date">
              <configuration name="headers" type="delimited" locale="fr"></configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>