<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="ihris-manage-CustomReports-facility-reports" path="/modules/CustomReports">
  <metadata>
    <version>4.3.0</version>
  </metadata>
  <configurationGroup name="ihris-manage-CustomReports-facility-reports" path="/modules/CustomReports" locale="en_US">
    <version>4.3.0</version>
    <configurationGroup name="relationships">
      <configurationGroup name="facility">
        <configuration name="display_name" locale="en_US">
          <value>Facility List</value>
        </configuration>
        <configuration name="description" locale="en_US">
          <value>Facility information</value>
        </configuration>
        <configurationGroup name="joins">
          <configurationGroup name="facility_type">
            <configuration name="description" locale="en_US">
              <value>The facility type for this facility.</value>
            </configuration>
            <configuration name="display_name" locale="en_US">
              <value>Facility Type</value>
            </configuration>
          </configurationGroup>
          <configurationGroup name="facility_contact">
            <configuration name="description" locale="en_US">
              <value>The contact information for this facility.</value>
            </configuration>
            <configuration name="display_name" locale="en_US">
              <value>Facility Contact</value>
            </configuration>
          </configurationGroup>
          <configurationGroup name="district">
            <configuration name="description" locale="en_US">
              <value>The district where this facility is.</value>
            </configuration>
            <configuration name="display_name" locale="en_US">
              <value>District</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="reports">
      <configurationGroup name="facility_list">
        <configurationGroup name="meta">
          <configuration name="display_name" locale="en_US">
            <value>Facility List</value>
          </configuration>
          <configuration name="description" locale="en_US">
            <value>Facility information.</value>
          </configuration>
          <configuration name="category" locale="en_US">
            <value>Facility Report</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="reporting_forms">
          <configurationGroup name="primary_form">
            <configurationGroup name="fields">
              <configurationGroup name="facility_type">
                <configurationGroup name="limits">
                  <configurationGroup name="equals">
                    <configuration name="header" locale="en_US">
                      <value>Facility Type</value>
                    </configuration>
                  </configurationGroup>
                </configurationGroup>
                <configuration name="header" locale="en_US">
                  <value>Facility Type ID</value>
                </configuration>
              </configurationGroup>
              <configurationGroup name="location">
                <configurationGroup name="limits">
                  <configurationGroup name="equals">
                    <configuration name="header" locale="en_US">
                      <value>Facility Location</value>
                    </configuration>
                  </configurationGroup>
                  <configurationGroup name="within">
                    <configuration name="header" locale="en_US">
                      <value>Facility Location</value>
                    </configuration>
                  </configurationGroup>
                </configurationGroup>
                <configuration name="header" locale="en_US">
                  <value>Facility Location ID</value>
                </configuration>
              </configurationGroup>
              <configurationGroup name="name">
                <configuration name="header" locale="en_US">
                  <value>Facility Name</value>
                </configuration>
              </configurationGroup>
            </configurationGroup>
          </configurationGroup>
          <configurationGroup name="district">
            <configurationGroup name="fields">
              <configurationGroup name="name">
                <configuration name="header" locale="en_US">
                  <value>Location</value>
                </configuration>
              </configurationGroup>
            </configurationGroup>
          </configurationGroup>
          <configurationGroup name="facility_contact">
            <configurationGroup name="fields">
              <configurationGroup name="address">
                <configuration name="header" locale="en_US">
                  <value>Mailing Address</value>
                </configuration>
              </configurationGroup>
              <configurationGroup name="telephone">
                <configuration name="header" locale="en_US">
                  <value>Telephone Number</value>
                </configuration>
              </configurationGroup>
              <configurationGroup name="email">
                <configuration name="header" locale="en_US">
                  <value>Email Address</value>
                </configuration>
              </configurationGroup>
              <configurationGroup name="notes">
                <configuration name="header" locale="en_US">
                  <value>Contact Notes</value>
                </configuration>
              </configurationGroup>
            </configurationGroup>
          </configurationGroup>
          <configurationGroup name="facility_type">
            <configurationGroup name="fields">
              <configurationGroup name="name">
                <configuration name="header" locale="en_US">
                  <value>Facility Type</value>
                </configuration>
              </configurationGroup>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="reportViews">
      <configurationGroup name="facility_list">
        <configuration name="description" locale="en_US">
          <value>A list of all facilities.</value>
        </configuration>
        <configuration name="display_name" locale="en_US">
          <value>Facility List</value>
        </configuration>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>