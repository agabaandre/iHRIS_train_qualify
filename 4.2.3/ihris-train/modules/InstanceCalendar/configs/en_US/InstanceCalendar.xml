<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="InstanceCalendar" path="/I2CE">
  <metadata>
    <version>4.2.1.0</version>
  </metadata>
  <configurationGroup name="InstanceCalendar" path="/I2CE" locale="en_US">
    <version>4.2.0</version>
    <configurationGroup name="tasks" path="tasks/task_description" locale="en_US">
      <configuration name="can_view_calendar_provider_instance">
        <value>View the Instance Calendar</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="instance_calendar">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="en_US">
            <value>Training Instance Calendar</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>