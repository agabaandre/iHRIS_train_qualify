<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="STSPerson" path="/I2CE">
  <metadata>
    <version>4.2.1.0</version>
  </metadata>
  <configurationGroup name="STSPerson" path="/I2CE" locale="en_US">
    <version>4.2.0.16</version>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_Person">
          <configurationGroup name="fields">
            <configurationGroup name="title">
              <configuration name="headers" type="delimited" locale="en_US">
                <value>default:Title</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="gender">
              <configuration name="headers" type="delimited" locale="en_US">
                <value>default:Gender</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="date_of_birth">
              <configuration name="headers" type="delimited" locale="en_US">
                <value>default:Date Of Birth</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="nationality">
              <configuration name="headers" type="delimited" locale="en_US">
                <value>default:Country of Citizenship</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="location">
              <configuration name="headers" type="delimited" locale="en_US">
                <value>default:Place Of Birth</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="person">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="en_US">
            <value>Add/Update Person</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>