<?xml version="1.0"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="mHero">
  <metadata>
    <displayName>mHero </displayName>
    <category>Application Component</category>
    <description>Provides CSD Interface for RapidPro</description>
    <creator>Intrahealth Informatics</creator>
    <email>hris@capacityproject.org</email>
    <link>https://launchpad.net/ihris-common</link>
    <version>4.2.0</version>
    <path name="classes">
      <value>./lib</value>
    </path>
    <requirement name="mHero-base">
      <atLeast version="4.2.0.1" />
      <lessThan version="4.3" />
    </requirement>
    <requirement name="Person-CSD">
      <atLeast version="4.2" />
      <lessThan version="4.3" />
    </requirement>
    <requirement name="mHero-Reports">
      <atLeast version="4.2" />
      <lessThan version="4.3" />
    </requirement>
  </metadata>

</I2CEConfiguration>
