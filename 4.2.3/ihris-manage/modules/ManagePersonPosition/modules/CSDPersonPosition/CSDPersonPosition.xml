<?xml version="1.0"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="ihris-manage-csd-PersonPosition">
  <metadata>
    <displayName>CSD Person Position</displayName>
    <className>iHRIS_Module_CSDPersonPosition</className>
    <category>Application Component</category>
    <description>CSD Person Position Configuration</description>
    <creator>Intrahealth Informatics</creator>
    <email>hris@capacityproject.org</email>
    <link>https://launchpad.net/ihris-manage</link>
    <version>4.3.4.0</version>
    <path name="configs">
      <value>./configs</value>
    </path>
    <path name="classes">
      <value>./lib</value>
    </path>
    <requirement name="csd_cache">
      <atLeast version="4.3" />
   
    </requirement>
    <requirement name="ihris-manage-PersonPosition">
      <atLeast version="4.3" />
  
    </requirement>
  </metadata>
  <configurationGroup name="ihris-manage-csd-PersonPosition" path="/">
    <configurationGroup name="csd_uuid" path="/modules/forms/formClasses/iHRIS_PersonPosition/fields/csd_uuid">
      <version>4.3.3.0</version>
      <configuration name="formfield">
        <value>STRING_LINE</value>
      </configuration>
      <configuration name="headers" type="delimited" locale="en_US">
        <value>default:CSD Entity ID</value>
      </configuration>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>