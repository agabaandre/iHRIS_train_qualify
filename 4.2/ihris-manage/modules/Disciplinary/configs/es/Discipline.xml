<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="disciplinary_action" path="/I2CE">
  <metadata>
    <version>4.3.0.1</version>
  </metadata>
  <configurationGroup name="disciplinary_action" path="/I2CE" locale="es">
    <version>4.3.0.1</version>
    <configurationGroup name="invalid_formfield_message" path="/modules/forms/formfield_messages" locale="es">
      <configuration name="disciplinary_date" path="disciplinary_action/end_date/bad_date">
        <value>La fecha final debe ser posterior a la fecha inicial.</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description">
      <configuration name="person_can_edit_child_form_disciplinary_action" locale="es">
        <value>Puede editar los accidentes del lugar de trabajo y las acciones_disciplinarias asignadas a una persona</value>
      </configuration>
      <configuration name="person_can_view_child_form_disciplinary_action" locale="es">
        <value>Puede visualizar los accidentes de trabajo y las acciones disciplinarias asignadas a una persona</value>
      </configuration>
      <configuration name="can_edit_database_list_disciplinary_action_type" locale="es">
        <value>Editar el tipo de lista de acciones disciplinarias del lugar de trabajo</value>
      </configuration>
      <configuration name="can_view_database_list_disciplinary_action_type" locale="es">
        <value>Visualizar la lista del tipo de acción disciplinaria del lugar de trabajo</value>
      </configuration>
      <configuration name="can_edit_database_list_disciplinary_action_reason" locale="es">
        <value>Editar la lista de tipo de motivos de acciones disciplinarias en el lugar de trabajo</value>
      </configuration>
      <configuration name="can_view_database_list_disciplinary_action_reason" locale="es">
        <value>Ver la lista de motivos de medidas disciplinarias en el lugar de trabajo</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="disciplinary_action">
          <configuration name="display" values="single" locale="es">
            <value>Acción disciplinaria</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="disciplinary_action_type">
          <configuration name="display" values="single" locale="es">
            <value>Tipo de acción disciplinaria</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="disciplinary_action_reason">
          <configuration name="display" values="single" locale="es">
            <value>Motivo de la Acción Disciplinaria</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_DisciplinaryAction">
          <configurationGroup name="fields">
            <configurationGroup name="action_date">
              <configuration name="headers" type="delimited" locale="es">
                <value>default:Fecha de discusión</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="disciplinary_action_type">
              <configuration name="headers" type="delimited" locale="es">
                <value>default:Acción tomada</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="disciplinary_action_reason">
              <configuration name="headers" type="delimited" locale="es">
                <value>default:Razón para la Acción</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="start_date">
              <configuration name="headers" type="delimited" locale="es">
                <value>default:Inicio de aplicabilidad</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="end_date">
              <configuration name="headers" type="delimited" locale="es">
                <value>default:Fin de aplicabilidad</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="notes">
              <configuration name="headers" type="delimited" locale="es">
                <value>default:Notas</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="persons_present">
              <configuration name="headers" type="delimited" locale="es">
                <value>default:Personas presentes</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="editAction" path="/modules/disciplinary_action/page/editAction">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="es">
            <value>Agregar/Actualizar la accicón disciplinaria</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>