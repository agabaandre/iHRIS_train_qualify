<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="ihris-manage-Salary" path="/I2CE">
  <metadata>
    <version>4.3.0.1</version>
  </metadata>
  <configurationGroup name="ihris-manage-Salary" path="/I2CE" locale="fr">
    <version>4.3.0.1</version>
    <configurationGroup name="invalid_formfield_message" path="/modules/forms/formfield_messages" locale="fr">
      <configuration name="sal_bad_date" path="salary/start_date/bad_date">
        <value>La date de début doit être postérieure à la date de début du salaire actuel.</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="salary">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="fr">
            <value>Modification du salaire</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="salary_history">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="fr">
            <value>Historique du salaire</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description">
      <configuration name="person_can_edit_child_form_salary" locale="fr">
        <value>Peut modifier le salaire d’une personne</value>
      </configuration>
      <configuration name="person_can_delete_child_form_salary" locale="fr">
        <value>Peut supprimer le salaire d’une personne</value>
      </configuration>
      <configuration name="person_can_change_child_form_salary" locale="fr">
        <value>Peut modifier le salaire actuel d’une personne</value>
      </configuration>
      <configuration name="person_can_view_child_form_salary" locale="fr">
        <value>Peut voir le salaire actuel d’une personne</value>
      </configuration>
      <configuration name="can_edit_database_list_salary_source" locale="fr">
        <value>Editer la liste des sources de salaire</value>
      </configuration>
      <configuration name="can_view_database_list_salary_source" locale="fr">
        <value>Afficher la liste des sources de salaire</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="salary">
          <configuration name="display" values="single" locale="fr">
            <value>Salaire</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="salary_source">
          <configuration name="display" values="single" locale="fr">
            <value>Source de revenu</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_Position">
          <configurationGroup name="fields">
            <configurationGroup name="proposed_salary">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:Salaire proposé</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="source">
              <configuration name="headers" type="delimited" locale="fr"></configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="iHRIS_Salary">
          <configurationGroup name="fields">
            <configurationGroup name="start_date">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:Date de début</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="salary">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:Salaire</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="end_date">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:Date de fin</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="notes">
              <configuration name="headers" type="delimited" locale="fr"></configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>