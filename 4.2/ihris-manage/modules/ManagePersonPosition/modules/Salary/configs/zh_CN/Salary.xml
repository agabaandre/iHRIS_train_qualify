<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="ihris-manage-Salary" path="/I2CE">
  <metadata>
    <version>4.2.2.0.1</version>
  </metadata>
  <configurationGroup name="ihris-manage-Salary" path="/I2CE" locale="zh_CN">
    <version>4.2.0.1</version>
    <configurationGroup name="invalid_formfield_message" path="/modules/forms/formfield_messages" locale="zh_CN">
      <configuration name="sal_bad_date" path="salary/start_date/bad_date"></configuration>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="salary">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="zh_CN">
            <value>薪水更改</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="salary_history">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="zh_CN">
            <value>薪水历史</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description">
      <configuration name="person_can_edit_child_form_salary" locale="zh_CN">
        <value>可以编辑一个人的薪水</value>
      </configuration>
      <configuration name="person_can_delete_child_form_salary" locale="zh_CN">
        <value>可以删除一个人的薪水</value>
      </configuration>
      <configuration name="person_can_change_child_form_salary" locale="zh_CN"></configuration>
      <configuration name="person_can_view_child_form_salary" locale="zh_CN"></configuration>
      <configuration name="can_edit_database_list_salary_source" locale="zh_CN">
        <value>编辑 salary_source 列表</value>
      </configuration>
      <configuration name="can_view_database_list_salary_source" locale="zh_CN">
        <value>查看 salary_source 列表</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="salary">
          <configuration name="display" values="single" locale="zh_CN">
            <value>薪水</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="salary_source">
          <configuration name="display" values="single" locale="zh_CN">
            <value>薪水来源</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_Position">
          <configurationGroup name="fields">
            <configurationGroup name="proposed_salary">
              <configuration name="headers" type="delimited" locale="zh_CN">
                <value>default:建议的薪水</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="source">
              <configuration name="headers" type="delimited" locale="zh_CN">
                <value>default:来源</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="iHRIS_Salary">
          <configurationGroup name="fields">
            <configurationGroup name="start_date">
              <configuration name="headers" type="delimited" locale="zh_CN">
                <value>default:开始日期</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="salary">
              <configuration name="headers" type="delimited" locale="zh_CN">
                <value>default:薪水</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="end_date">
              <configuration name="headers" type="delimited" locale="zh_CN">
                <value>default:结束日期</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="notes">
              <configuration name="headers" type="delimited" locale="zh_CN">
                <value>default:笔记</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>