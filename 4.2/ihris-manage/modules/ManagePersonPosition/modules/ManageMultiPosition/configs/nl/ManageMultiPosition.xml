<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="ihris-manage-MultiPosition" path="/I2CE">
  <metadata>
    <version>4.2.2.0.1</version>
  </metadata>
  <configurationGroup name="ihris-manage-MultiPosition" path="/I2CE" locale="nl">
    <version>4.2.0.1</version>
    <configurationGroup name="invalid_formfield_message" path="/modules/forms/formfield_messages" locale="nl">
      <configuration name="code_req" path="position/code_start/required">
        <value>Als u het toevoegen van meerdere positie moet u een nummer voor de code om te starten met.</value>
      </configuration>
      <configuration name="code_req" path="position/code_format/unique">
        <value>Een positie-code bestaat al voor een van de posities die u probeert toe te voegen. Selecteer een ander formaat van de code of start met een hoger nummer.</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_MultiPosition">
          <configurationGroup name="fields">
            <configurationGroup name="count">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:Aantal Posities te Maken</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="code_start">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:Nummer om mee te Starten voor de Positie van de Code</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="code_format">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:Het Formaat van de Code te gebruiken met het Aantal</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>