<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="ihris-manage-MultiPosition" path="/I2CE">
  <metadata>
    <version>4.2.2.0.1</version>
  </metadata>
  <configurationGroup name="ihris-manage-MultiPosition" path="/I2CE" locale="cs">
    <version>4.2.0.1</version>
    <configurationGroup name="invalid_formfield_message" path="/modules/forms/formfield_messages" locale="cs">
      <configuration name="code_req" path="position/code_start/required">
        <value>Pokud přidáváte mnoho pozicí, musíte zadat číslo pro kód se kterou začít.</value>
      </configuration>
      <configuration name="code_req" path="position/code_format/unique">
        <value>Kód pozice již existuje pro jednu z pozic, kterou se snažíte přidat. Prosím vyberte jiný formát kódu nebo začněte s vyšším číslem.</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_MultiPosition">
          <configurationGroup name="fields">
            <configurationGroup name="count">
              <configuration name="headers" type="delimited" locale="cs">
                <value>default:Počet pozic k vytvoření</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="code_start">
              <configuration name="headers" type="delimited" locale="cs">
                <value>default:Číslo, kterým začít kód pozic</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="code_format">
              <configuration name="headers" type="delimited" locale="cs">
                <value>default:Formát kódu, který použít s číslem</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>