<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="accident" path="/I2CE">
  <metadata>
    <version>4.3.0.1</version>
  </metadata>
  <configurationGroup name="accident" path="/I2CE" locale="et">
    <version>4.3.0.1</version>
    <configurationGroup name="invalid_formfield_message" path="/modules/forms/formfield_messages" locale="et">
      <configuration name="accident_date" path="accident/end_date/bad_date"></configuration>
    </configurationGroup>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description">
      <configuration name="person_can_edit_child_form_accident" locale="et">
        <value>Saab redigeerida töövigastusi- ja õnnetusi kirjutatud inimestele</value>
      </configuration>
      <configuration name="person_can_view_child_form_accident" locale="et">
        <value>Saab vaadata töövigastusi- ja õnnetusi kirjutatud inimestele</value>
      </configuration>
      <configuration name="can_edit_database_list_accident_type" locale="et">
        <value>Redigeeri tööõnnetuste tüüpide nimekirja</value>
      </configuration>
      <configuration name="can_view_database_list_accident_type" locale="et">
        <value>Vaata tööõnnetuste tüüpide tabelit</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="accident">
          <configuration name="display" values="single" locale="et">
            <value>Õnnetus</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="accident_type">
          <configuration name="display" values="single" locale="et">
            <value>Tööõnnetuse tüüp</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_Accident">
          <configurationGroup name="fields">
            <configurationGroup name="occurence_date">
              <configuration name="headers" type="delimited" locale="et">
                <value>default:Juhtumise kuupäev</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="accident_type">
              <configuration name="headers" type="delimited" locale="et">
                <value>default:Õnnetuse tüüp</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="start_date">
              <configuration name="headers" type="delimited" locale="et">
                <value>default:Kohaldatavuse algus</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="end_date">
              <configuration name="headers" type="delimited" locale="et">
                <value>default:Kohaldatavuse lõpp</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="followup">
              <configuration name="headers" type="delimited" locale="et">
                <value>default:Järg vajalik</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="persons_involved">
              <configuration name="headers" type="delimited" locale="et">
                <value>default:Seotud inimesed</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="home" path="/modules/accident/page/home">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="et">
            <value>Lisa/Uuenda tööõnnetus</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>