<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="accident" path="/I2CE">
  <metadata>
    <version>4.3.0.1</version>
  </metadata>
  <configurationGroup name="accident" path="/I2CE" locale="it">
    <version>4.3.0.1</version>
    <configurationGroup name="invalid_formfield_message" path="/modules/forms/formfield_messages" locale="it">
      <configuration name="accident_date" path="accident/end_date/bad_date">
        <value>La data di fine deve essere successiva alla data di inizio.</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description">
      <configuration name="person_can_edit_child_form_accident" locale="it">
        <value>Modifica infortuni e incidenti sul lavoro assegnati a una persona</value>
      </configuration>
      <configuration name="person_can_view_child_form_accident" locale="it">
        <value>Visualizza infortuni e incidenti sul lavoro assegnati a una persona</value>
      </configuration>
      <configuration name="can_edit_database_list_accident_type" locale="it">
        <value>Modifica l'elenco delle tipologie di incidenti sul lavoro</value>
      </configuration>
      <configuration name="can_view_database_list_accident_type" locale="it">
        <value>Vedi la lista degli Infortuni sul Posto di Lavoro</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="accident">
          <configuration name="display" values="single" locale="it">
            <value>Incidente</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="accident_type">
          <configuration name="display" values="single" locale="it">
            <value>Tipologia di incidente sul lavoro</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_Accident">
          <configurationGroup name="fields">
            <configurationGroup name="occurence_date">
              <configuration name="headers" type="delimited" locale="it">
                <value>default:Data dell'evento</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="accident_type">
              <configuration name="headers" type="delimited" locale="it">
                <value>default:Tipo di incidente</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="start_date">
              <configuration name="headers" type="delimited" locale="it">
                <value>default:Inizio dell'applicabilità</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="end_date">
              <configuration name="headers" type="delimited" locale="it">
                <value>default:Fine dell'applicabilità</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="followup">
              <configuration name="headers" type="delimited" locale="it">
                <value>default:Visita di controllo richiesta</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="persons_involved">
              <configuration name="headers" type="delimited" locale="it">
                <value>default:Persone coinvolte</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="home" path="/modules/accident/page/home">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="it">
            <value>Aggiungi/aggiorna incidente sul lavoro</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>