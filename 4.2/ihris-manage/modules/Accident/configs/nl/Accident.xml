<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="accident" path="/I2CE">
  <metadata>
    <version>4.3.0.1</version>
  </metadata>
  <configurationGroup name="accident" path="/I2CE" locale="nl">
    <version>4.3.0.1</version>
    <configurationGroup name="invalid_formfield_message" path="/modules/forms/formfield_messages" locale="nl">
      <configuration name="accident_date" path="accident/end_date/bad_date">
        <value>De einddatum moet na de begindatum liggen.</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description">
      <configuration name="person_can_edit_child_form_accident" locale="nl">
        <value>Kan de arbeidsongevallen voor een persoon bewerken</value>
      </configuration>
      <configuration name="person_can_view_child_form_accident" locale="nl">
        <value>Kan de arbeidsongevallen voor een persoon bekijken</value>
      </configuration>
      <configuration name="can_edit_database_list_accident_type" locale="nl">
        <value>De lijst met typen arbeidsongevallen bewerken</value>
      </configuration>
      <configuration name="can_view_database_list_accident_type" locale="nl">
        <value>De lijst met typen arbeidsongevallen bekijken</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="accident">
          <configuration name="display" values="single" locale="nl">
            <value>Ongeval</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="accident_type">
          <configuration name="display" values="single" locale="nl">
            <value>Type arbeidsongeval</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_Accident">
          <configurationGroup name="fields">
            <configurationGroup name="occurence_date">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:Datum ongeval</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="accident_type">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:Type ongeval</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="start_date">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:Van toepassing vanaf</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="end_date">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:Van toepassing tot</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="followup">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:Nabehandeling benodigd</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="persons_involved">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:Betrokken personen</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="home" path="/modules/accident/page/home">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="nl">
            <value>Arbeidsongeval toevoegen of bijwerken</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>