<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="accident" path="/I2CE">
  <metadata>
    <version>4.2.2.0.1</version>
  </metadata>
  <configurationGroup name="accident" path="/I2CE" locale="ta">
    <version>4.2.0.1</version>
    <configurationGroup name="invalid_formfield_message" path="/modules/forms/formfield_messages" locale="ta">
      <configuration name="accident_date" path="accident/end_date/bad_date"></configuration>
    </configurationGroup>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description">
      <configuration name="person_can_edit_child_form_accident" locale="ta">
        <value>ஒரு நபருக்காக ஒதுக்கப்படும் பணியிட காயங்கள் மற்றும் நேர்ச்சிகளை திருத்த முடியும்</value>
      </configuration>
      <configuration name="person_can_view_child_form_accident" locale="ta">
        <value>ஒரு நபருக்காக ஒதுக்கப்படும் பணியிட காயங்கள் மற்றும் நேர்ச்சிகளை திருத்த முடியும்</value>
      </configuration>
      <configuration name="can_edit_database_list_accident_type" locale="ta">
        <value>பணியிடத்திலுல்ல நேர்ச்சி வகைப் பட்டியலை திருத்தவும்</value>
      </configuration>
      <configuration name="can_view_database_list_accident_type" locale="ta"></configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="accident">
          <configuration name="display" values="single" locale="ta">
            <value>நேர்ச்சி (விபத்து)</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="accident_type">
          <configuration name="display" values="single" locale="ta">
            <value>பணியிடத்தில் நேர்ச்சி (விபத்து) வகை</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_Accident">
          <configurationGroup name="fields">
            <configurationGroup name="occurence_date">
              <configuration name="headers" type="delimited" locale="ta">
                <value>default:நிகழ்ந்த நாள்</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="accident_type">
              <configuration name="headers" type="delimited" locale="ta">
                <value>default:நேர்ச்சியின் (விபத்தின்) வகை</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="start_date">
              <configuration name="headers" type="delimited" locale="ta">
                <value>default:பயண்படுத்தத்தக்க தொடக்கம்</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="end_date">
              <configuration name="headers" type="delimited" locale="ta">
                <value>default:பயன்படுத்தத்தக்க முடிவு</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="followup">
              <configuration name="headers" type="delimited" locale="ta">
                <value>default:பின்தொடர்தல் தேவை</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="persons_involved">
              <configuration name="headers" type="delimited" locale="ta">
                <value>default:தொடர்புடைய மக்கள்</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="home" path="/modules/accident/page/home">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="ta">
            <value>பணியிட விபத்தை சேர்/புதுப்பி</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>