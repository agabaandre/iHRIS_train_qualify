<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="accident" path="/I2CE">
  <metadata>
    <version>4.3.0.1</version>
  </metadata>
  <configurationGroup name="accident" path="/I2CE" locale="pt_BR">
    <version>4.3.0.1</version>
    <configurationGroup name="invalid_formfield_message" path="/modules/forms/formfield_messages" locale="pt_BR">
      <configuration name="accident_date" path="accident/end_date/bad_date">
        <value>A data final precisa estar após a data inicial.</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description">
      <configuration name="person_can_edit_child_form_accident" locale="pt_BR">
        <value>Pode editar as injúrias e acidentes de trabalho relacionados à uma pessoa</value>
      </configuration>
      <configuration name="person_can_view_child_form_accident" locale="pt_BR">
        <value>Pode ver as injúrias e acidentes de trabalho relacionados à uma pessoa</value>
      </configuration>
      <configuration name="can_edit_database_list_accident_type" locale="pt_BR">
        <value>Editar a lista de acidentes de trabalho</value>
      </configuration>
      <configuration name="can_view_database_list_accident_type" locale="pt_BR">
        <value>Ver a lista de tipos de acidentes de trabalho</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="accident">
          <configuration name="display" values="single" locale="pt_BR">
            <value>Acidente</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="accident_type">
          <configuration name="display" values="single" locale="pt_BR">
            <value>Tipo de Acidente de Trabalho</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_Accident">
          <configurationGroup name="fields">
            <configurationGroup name="occurence_date">
              <configuration name="headers" type="delimited" locale="pt_BR">
                <value>default:Data da Ocorrência</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="accident_type">
              <configuration name="headers" type="delimited" locale="pt_BR">
                <value>default:Tipo de Acidente</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="start_date">
              <configuration name="headers" type="delimited" locale="pt_BR">
                <value>default:Início da Aplicabilidade</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="end_date">
              <configuration name="headers" type="delimited" locale="pt_BR">
                <value>default:Fim da Aplicabilidade</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="followup">
              <configuration name="headers" type="delimited" locale="pt_BR">
                <value>default:Acompanhamento Requerido</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="persons_involved">
              <configuration name="headers" type="delimited" locale="pt_BR">
                <value>default:Pessoas Envolvidas</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="home" path="/modules/accident/page/home">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="pt_BR">
            <value>Adicionar/Atualizar Acidentes de Trabalho</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>