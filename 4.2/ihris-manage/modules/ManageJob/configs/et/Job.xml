<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="ihris-manage-Job" path="/I2CE">
  <metadata>
    <version>4.3.0.1</version>
  </metadata>
  <configurationGroup name="ihris-manage-Job" path="/I2CE" locale="et">
    <version>4.3.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description">
      <configuration name="can_edit_database_list_salary_grade" locale="et">
        <value>Redigeeri palgatase nimekirja</value>
      </configuration>
      <configuration name="can_view_database_list_salary_grade" locale="et">
        <value>Vaata palgatase nimekirja</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="view_job">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="et">
            <value>Vaata töökohta</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="salary_grade">
          <configuration name="display" values="single" locale="et">
            <value>Palgatase</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_SalaryGrade">
          <configurationGroup name="fields">
            <configurationGroup name="name">
              <configuration name="headers" type="delimited" locale="et">
                <value>default:Nimi</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="start">
              <configuration name="headers" type="delimited" locale="et">
                <value>default:Alusta</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="end">
              <configuration name="headers" type="delimited" locale="et">
                <value>default:Lõpeta</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="midpoint">
              <configuration name="headers" type="delimited" locale="et">
                <value>default:KeskPunkt</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="notes">
              <configuration name="headers" type="delimited" locale="et">
                <value>default:Märkmed</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="iHRIS_ManageJob">
          <configurationGroup name="fields">
            <configurationGroup name="salary_grade">
              <configuration name="headers" type="delimited" locale="et">
                <value>default:Palgatase</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>