<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="ihris-manage-Job" path="/I2CE">
  <metadata>
    <version>4.3.0.1</version>
  </metadata>
  <configurationGroup name="ihris-manage-Job" path="/I2CE" locale="de">
    <version>4.3.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description">
      <configuration name="can_edit_database_list_salary_grade" locale="de">
        <value>Liste der Entgeltstufen bearbeiten</value>
      </configuration>
      <configuration name="can_view_database_list_salary_grade" locale="de">
        <value>Liste der Entgeltstufen einsehen</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="view_job">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="de">
            <value>Stelle einsehen</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="salary_grade">
          <configuration name="display" values="single" locale="de">
            <value>Entgeltstufe</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_SalaryGrade">
          <configurationGroup name="fields">
            <configurationGroup name="name">
              <configuration name="headers" type="delimited" locale="de"></configuration>
            </configurationGroup>
            <configurationGroup name="start">
              <configuration name="headers" type="delimited" locale="de">
                <value>default:Anfang</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="end">
              <configuration name="headers" type="delimited" locale="de">
                <value>default:Ende</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="midpoint">
              <configuration name="headers" type="delimited" locale="de">
                <value>default:Mittelwert</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="notes">
              <configuration name="headers" type="delimited" locale="de">
                <value>default:Notizen</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="iHRIS_ManageJob">
          <configurationGroup name="fields">
            <configurationGroup name="salary_grade">
              <configuration name="headers" type="delimited" locale="de">
                <value>default:Entgeltstufe</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>