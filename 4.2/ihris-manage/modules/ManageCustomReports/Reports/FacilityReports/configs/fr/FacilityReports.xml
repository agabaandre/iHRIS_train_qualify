<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="ihris-manage-CustomReports-facility-reports" path="/modules/CustomReports">
  <metadata>
    <version>4.2.2.0.1</version>
  </metadata>
  <configurationGroup name="ihris-manage-CustomReports-facility-reports" path="/modules/CustomReports" locale="fr">
    <version>4.2.0.1</version>
    <configurationGroup name="relationships">
      <configurationGroup name="facility">
        <configuration name="display_name" locale="fr">
          <value>Liste des établissements</value>
        </configuration>
        <configuration name="description" locale="fr">
          <value>Informations sur les établissements</value>
        </configuration>
        <configurationGroup name="joins">
          <configurationGroup name="facility_type">
            <configuration name="description" locale="fr">
              <value>Le type d’établissement pour cet établissement.</value>
            </configuration>
            <configuration name="display_name" locale="fr">
              <value>Type d’établissement</value>
            </configuration>
          </configurationGroup>
          <configurationGroup name="facility_contact">
            <configuration name="description" locale="fr">
              <value>Les coordonnées pour cet établissement.</value>
            </configuration>
            <configuration name="display_name" locale="fr">
              <value>Contact de l’établissement</value>
            </configuration>
          </configurationGroup>
          <configurationGroup name="district">
            <configuration name="description" locale="fr">
              <value>Le district où se trouve l’établissement.</value>
            </configuration>
            <configuration name="display_name" locale="fr"></configuration>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="reports">
      <configurationGroup name="facility_list">
        <configurationGroup name="meta">
          <configuration name="display_name" locale="fr">
            <value>Liste des établissements</value>
          </configuration>
          <configuration name="description" locale="fr">
            <value>Informations sur l’établissement.</value>
          </configuration>
          <configuration name="category" locale="fr">
            <value>Rapport sur l’établissement</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="reporting_forms">
          <configurationGroup name="primary_form">
            <configurationGroup name="fields">
              <configurationGroup name="facility_type">
                <configurationGroup name="limits">
                  <configurationGroup name="equals">
                    <configuration name="header" locale="fr">
                      <value>Type d’établissement</value>
                    </configuration>
                  </configurationGroup>
                </configurationGroup>
                <configuration name="header" locale="fr">
                  <value>Type d’ID de l’établissement</value>
                </configuration>
              </configurationGroup>
              <configurationGroup name="location">
                <configurationGroup name="limits">
                  <configurationGroup name="equals">
                    <configuration name="header" locale="fr">
                      <value>Emplacement de l’Etablissement</value>
                    </configuration>
                  </configurationGroup>
                  <configurationGroup name="within">
                    <configuration name="header" locale="fr">
                      <value>Emplacement de l’Etablissement</value>
                    </configuration>
                  </configurationGroup>
                </configurationGroup>
                <configuration name="header" locale="fr">
                  <value>ID de l’emplacement de l’établissement</value>
                </configuration>
              </configurationGroup>
              <configurationGroup name="name">
                <configuration name="header" locale="fr">
                  <value>Nom de l’établissement</value>
                </configuration>
              </configurationGroup>
            </configurationGroup>
          </configurationGroup>
          <configurationGroup name="district">
            <configurationGroup name="fields">
              <configurationGroup name="name">
                <configuration name="header" locale="fr">
                  <value>Emplacement</value>
                </configuration>
              </configurationGroup>
            </configurationGroup>
          </configurationGroup>
          <configurationGroup name="facility_contact">
            <configurationGroup name="fields">
              <configurationGroup name="address">
                <configuration name="header" locale="fr">
                  <value>Adresse postale</value>
                </configuration>
              </configurationGroup>
              <configurationGroup name="telephone">
                <configuration name="header" locale="fr">
                  <value>Numéro de téléphone</value>
                </configuration>
              </configurationGroup>
              <configurationGroup name="email">
                <configuration name="header" locale="fr">
                  <value>Courriel</value>
                </configuration>
              </configurationGroup>
              <configurationGroup name="notes">
                <configuration name="header" locale="fr">
                  <value>Commentaires sur les contacts</value>
                </configuration>
              </configurationGroup>
            </configurationGroup>
          </configurationGroup>
          <configurationGroup name="facility_type">
            <configurationGroup name="fields">
              <configurationGroup name="name">
                <configuration name="header" locale="fr">
                  <value>Type d’établissement</value>
                </configuration>
              </configurationGroup>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="reportViews">
      <configurationGroup name="facility_list">
        <configuration name="description" locale="fr">
          <value>Une liste de tous les établissements.</value>
        </configuration>
        <configuration name="display_name" locale="fr">
          <value>Liste des établissements</value>
        </configuration>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>