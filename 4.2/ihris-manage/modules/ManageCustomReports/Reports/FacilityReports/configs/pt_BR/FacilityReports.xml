                  <value>Número de Telefone</value>
                </configuration>
              </configurationGroup>
              <configurationGroup name="email">
                <configuration name="header" locale="pt_BR">
                  <value>E-mail</value>
                </configuration>
              </configurationGroup>
              <configurationGroup name="notes">
                <configuration name="header" locale="pt_BR">
                  <value>Notas do Contato</value>
                </configuration>
              </configurationGroup>
            </configurationGroup>
          </configurationGroup>
          <configurationGroup name="facility_type">
            <configurationGroup name="fields">
              <configurationGroup name="name">
                <configuration name="header" locale="pt_BR">
                  <value>Tipo de Escritório</value>
                </configuration>
              </configurationGroup>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="reportViews">
      <configurationGroup name="facility_list">
        <configuration name="description" locale="pt_BR">
          <value>Uma lista de todos os escritórios.</value>
        </configuration>
        <configuration name="display_name" locale="pt_BR">
          <value>Lista de Escritórios</value>
        </configuration>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>