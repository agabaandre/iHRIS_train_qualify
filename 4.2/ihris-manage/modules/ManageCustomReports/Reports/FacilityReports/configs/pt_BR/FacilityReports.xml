<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="ihris-manage-CustomReports-facility-reports" path="/modules/CustomReports">
  <metadata>
    <version>4.2.2.0.1</version>
  </metadata>
  <configurationGroup name="ihris-manage-CustomReports-facility-reports" path="/modules/CustomReports" locale="pt_BR">
    <version>4.2.0.1</version>
    <configurationGroup name="relationships">
      <configurationGroup name="facility">
        <configuration name="display_name" locale="pt_BR">
          <value>Lista de Escritórios</value>
        </configuration>
        <configuration name="description" locale="pt_BR">
          <value>Informação do Escritório</value>
        </configuration>
        <configurationGroup name="joins">
          <configurationGroup name="facility_type">
            <configuration name="description" locale="pt_BR">
              <value>O tipo de escritório para esse escritório.</value>
            </configuration>
            <configuration name="display_name" locale="pt_BR">
              <value>Tipo de Escritório</value>
            </configuration>
          </configurationGroup>
          <configurationGroup name="facility_contact">
            <configuration name="description" locale="pt_BR">
              <value>As informações de contato para esse escritório.</value>
            </configuration>
            <configuration name="display_name" locale="pt_BR">
              <value>Contato do Escritório</value>
            </configuration>
          </configurationGroup>
          <configurationGroup name="district">
