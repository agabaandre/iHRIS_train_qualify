              <value>O bairro onde o escritório está.</value>
            </configuration>
            <configuration name="display_name" locale="pt_BR">
              <value>Bairro</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="reports">
      <configurationGroup name="facility_list">
        <configurationGroup name="meta">
          <configuration name="display_name" locale="pt_BR">
            <value>Lista de Escritórios</value>
          </configuration>
          <configuration name="description" locale="pt_BR">
            <value>Informação do Escritório.</value>
          </configuration>
          <configuration name="category" locale="pt_BR">
            <value>Relatório do Escritório</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="reporting_forms">
          <configurationGroup name="primary_form">
            <configurationGroup name="fields">
              <configurationGroup name="facility_type">
                <configurationGroup name="limits">
                  <configurationGroup name="equals">
                    <configuration name="header" locale="pt_BR">
                      <value>Tipo de Escritório</value>
                    </configuration>
                  </configurationGroup>
                </configurationGroup>
                <configuration name="header" locale="pt_BR">
                  <value>Identificação do Tipo de Escritório</value>
                </configuration>
              </configurationGroup>
              <configurationGroup name="location">
                <configurationGroup name="limits">
                  <configurationGroup name="equals">
                    <configuration name="header" locale="pt_BR">
                      <value>Localização do Escritório</value>
                    </configuration>
                  </configurationGroup>
                  <configurationGroup name="within">
                    <configuration name="header" locale="pt_BR">
                      <value>Localização do Escritório</value>
                    </configuration>
                  </configurationGroup>
                </configurationGroup>
                <configuration name="header" locale="pt_BR">
                  <value>Identificação da Localização do Escritório</value>
                </configuration>
              </configurationGroup>
              <configurationGroup name="name">
                <configuration name="header" locale="pt_BR">
                  <value>Nome do Escritório</value>
                </configuration>
              </configurationGroup>
            </configurationGroup>
          </configurationGroup>
          <configurationGroup name="district">
            <configurationGroup name="fields">
              <configurationGroup name="name">
                <configuration name="header" locale="pt_BR">
                  <value>Localização</value>
                </configuration>
              </configurationGroup>
            </configurationGroup>
          </configurationGroup>
          <configurationGroup name="facility_contact">
            <configurationGroup name="fields">
              <configurationGroup name="address">
                <configuration name="header" locale="pt_BR">
                  <value>Endereço de Correspondência</value>
                </configuration>
              </configurationGroup>
              <configurationGroup name="telephone">
