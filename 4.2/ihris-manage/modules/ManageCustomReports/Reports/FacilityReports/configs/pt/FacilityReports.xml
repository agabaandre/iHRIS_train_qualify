                </configuration>
              </configurationGroup>
            </configurationGroup>
          </configurationGroup>
          <configurationGroup name="facility_type">
            <configurationGroup name="fields">
              <configurationGroup name="name">
                <configuration name="header" locale="pt">
                  <value>Tipo de Unidade</value>
                </configuration>
              </configurationGroup>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
