          <value>Lista de Unidades</value>
        </configuration>
        <configuration name="description" locale="pt">
          <value>Informação sobre Unidades</value>
        </configuration>
        <configurationGroup name="joins">
          <configurationGroup name="facility_type">
            <configuration name="description" locale="pt">
              <value>O Tipo de Unidade desta Unidade.</value>
            </configuration>
            <configuration name="display_name" locale="pt">
              <value>Tipo de Unidade</value>
            </configuration>
          </configurationGroup>
          <configurationGroup name="facility_contact">
