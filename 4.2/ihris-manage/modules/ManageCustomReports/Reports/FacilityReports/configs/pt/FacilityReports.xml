    <configurationGroup name="reportViews">
      <configurationGroup name="facility_list">
        <configuration name="description" locale="pt">
          <value>Uma lista de todas as Unidades</value>
        </configuration>
        <configuration name="display_name" locale="pt">
          <value>Lista de Unidades</value>
