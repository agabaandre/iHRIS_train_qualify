<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="ihris-manage-Benefit" path="/I2CE">
  <metadata>
    <version>4.2.2.0.1</version>
  </metadata>
  <configurationGroup name="ihris-manage-Benefit" path="/I2CE" locale="zh_CN">
    <version>4.2.0.1</version>
    <configurationGroup name="invalid_formfield_message" path="/modules/forms/formfield_messages" locale="zh_CN">
      <configuration name="benefit_date" path="benefit/end_date/bad_date">
        <value>结束日期必须在开始日期之后。</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="zh_CN">
      <configuration name="person_can_view_child_form_benefit" locale="zh_CN">
        <value>可以查看某人的利益子窗体</value>
      </configuration>
      <configuration name="person_can_edit_child_form_benefit" locale="zh_CN">
        <value>可以编辑某人的利益子窗体</value>
      </configuration>
      <configuration name="can_edit_database_list_benefit_type" locale="zh_CN">
        <value>可以编辑效益类型数据库列表</value>
      </configuration>
      <configuration name="can_view_database_list_benefit_type" locale="zh_CN">
        <value>可以查看效益类型数据库列表</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="formsData" path="formsData/forms">
      <configurationGroup name="benefit_recurrence">
        <configurationGroup name="once">
          <configuration name="fields" value="many" type="delimited" locale="zh_CN">
            <value>name:一次</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="weekly">
          <configuration name="fields" value="many" type="delimited" locale="zh_CN">
            <value>name:每周</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="monthly">
          <configuration name="fields" value="many" type="delimited" locale="zh_CN">
            <value>name:每月</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="yearly">
          <configuration name="fields" value="many" type="delimited" locale="zh_CN">
            <value>name:每年</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="benefit">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="zh_CN">
            <value>添加/更新的好处</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="benefit">
          <configuration name="display" values="single" locale="zh_CN">
            <value>效益</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="benefit_type">
          <configuration name="display" values="single" locale="zh_CN">
            <value>效益类型</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="benefit_recurrence">
          <configuration name="display" values="single" locale="zh_CN">
            <value>效益复发</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_Benefit">
          <configurationGroup name="fields">
            <configurationGroup name="type">
              <configuration name="headers" type="delimited" locale="zh_CN">
                <value>default:效益类型</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="amount">
              <configuration name="headers" type="delimited" locale="zh_CN">
                <value>default:金额</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="source">
              <configuration name="headers" type="delimited" locale="zh_CN">
                <value>default:源</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="start_date">
              <configuration name="headers" type="delimited" locale="zh_CN">
                <value>default:开始日期</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="end_date">
              <configuration name="headers" type="delimited" locale="zh_CN">
                <value>default:结束日期</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="recurrence">
              <configuration name="headers" type="delimited" locale="zh_CN">
                <value>default:重复频率</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>