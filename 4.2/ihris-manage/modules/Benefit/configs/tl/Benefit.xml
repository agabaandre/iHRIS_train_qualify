<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="ihris-manage-Benefit" path="/I2CE">
  <metadata>
    <version>4.3.0.1</version>
  </metadata>
  <configurationGroup name="ihris-manage-Benefit" path="/I2CE" locale="tl">
    <version>4.3.0.1</version>
    <configurationGroup name="invalid_formfield_message" path="/modules/forms/formfield_messages" locale="tl">
      <configuration name="benefit_date" path="benefit/end_date/bad_date">
        <value>Ang petsa ng pagwawakas ay dapat na maging pagkalipas ng petsa ng pagsisimula.</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="tl">
      <configuration name="person_can_view_child_form_benefit" locale="tl">
        <value>Makikita ang pormularyo ng batang may benepisyo ng isang tao</value>
      </configuration>
      <configuration name="person_can_edit_child_form_benefit" locale="tl">
        <value>Mababago ang mga pormularyo ng batang may benepisyo ng isang tao</value>
      </configuration>
      <configuration name="can_edit_database_list_benefit_type" locale="tl">
        <value>Mababago ang talaang pangkalipunan ng dato ng uri ng benepisyo</value>
      </configuration>
      <configuration name="can_view_database_list_benefit_type" locale="tl">
        <value>Matatanaw ang talaang pangkalipunan ng dato ng uri ng benepisyo</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="formsData" path="formsData/forms">
      <configurationGroup name="benefit_recurrence">
        <configurationGroup name="once">
          <configuration name="fields" value="many" type="delimited" locale="tl">
            <value>name:Isang ulit</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="weekly">
          <configuration name="fields" value="many" type="delimited" locale="tl">
            <value>name:Lingguhan</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="monthly">
          <configuration name="fields" value="many" type="delimited" locale="tl">
            <value>name:Buwanan</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="yearly">
          <configuration name="fields" value="many" type="delimited" locale="tl">
            <value>name:Taunan</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="benefit">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="tl">
            <value>Magdagdag/Magsapanahon ng mga Benepisyo</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="benefit">
          <configuration name="display" values="single" locale="tl">
            <value>Benepisyo</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="benefit_type">
          <configuration name="display" values="single" locale="tl">
            <value>Uri ng Benepisyo</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="benefit_recurrence">
          <configuration name="display" values="single" locale="tl">
            <value>Pag-uulit ng Benepisyo</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_Benefit">
          <configurationGroup name="fields">
            <configurationGroup name="type">
              <configuration name="headers" type="delimited" locale="tl">
                <value>default:Uri ng Benepisyo</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="amount">
              <configuration name="headers" type="delimited" locale="tl">
                <value>default:Dami</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="source">
              <configuration name="headers" type="delimited" locale="tl">
                <value>default:Pinagkunan</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="start_date">
              <configuration name="headers" type="delimited" locale="tl">
                <value>default:Petsa ng Simula</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="end_date">
              <configuration name="headers" type="delimited" locale="tl">
                <value>default:Petsa ng Pagtatapos</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="recurrence">
              <configuration name="headers" type="delimited" locale="tl">
                <value>default:Dalas ng Pag-uulit</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>