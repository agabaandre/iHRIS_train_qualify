<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="ihris-manage-Benefit" path="/I2CE">
  <metadata>
    <version>4.2.2.0.1</version>
  </metadata>
  <configurationGroup name="ihris-manage-Benefit" path="/I2CE" locale="fr_ML">
    <version>4.2.0.1</version>
    <configurationGroup name="invalid_formfield_message" path="/modules/forms/formfield_messages" locale="fr_ML">
      <configuration name="benefit_date" path="benefit/end_date/bad_date"></configuration>
    </configurationGroup>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="fr_ML">
      <configuration name="person_can_view_child_form_benefit" locale="fr_ML">
        <value>Peut afficher le formulaire enfant de l’allocation d’une personne</value>
      </configuration>
      <configuration name="person_can_edit_child_form_benefit" locale="fr_ML">
        <value>Peut éditer le formulaire enfant de l’avantage d’une personne</value>
      </configuration>
      <configuration name="can_edit_database_list_benefit_type" locale="fr_ML">
        <value>Peut éditer la liste de base de données des types d’avantage</value>
      </configuration>
      <configuration name="can_view_database_list_benefit_type" locale="fr_ML">
        <value>Peut afficher la liste de la base des données de type d’avantage</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="formsData" path="formsData/forms">
      <configurationGroup name="benefit_recurrence">
        <configurationGroup name="once">
          <configuration name="fields" value="many" type="delimited" locale="fr_ML">
            <value>name:Une fois</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="weekly">
          <configuration name="fields" value="many" type="delimited" locale="fr_ML">
            <value>name:Hebdomadairement</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="monthly">
          <configuration name="fields" value="many" type="delimited" locale="fr_ML">
            <value>name:Mensuel</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="yearly">
          <configuration name="fields" value="many" type="delimited" locale="fr_ML">
            <value>name:Annuellement</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="benefit">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="fr_ML">
            <value>Ajouter/Mettre à jour les avantages</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="benefit">
          <configuration name="display" values="single" locale="fr_ML">
            <value>Avantages</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="benefit_type">
          <configuration name="display" values="single" locale="fr_ML">
            <value>Type d'avantage</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="benefit_recurrence">
          <configuration name="display" values="single" locale="fr_ML">
            <value>Répétition des avantages</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_Benefit">
          <configurationGroup name="fields">
            <configurationGroup name="type">
              <configuration name="headers" type="delimited" locale="fr_ML">
                <value>default:Type d'avantage</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="amount">
              <configuration name="headers" type="delimited" locale="fr_ML">
                <value>default:Montant</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="source">
              <configuration name="headers" type="delimited" locale="fr_ML"></configuration>
            </configurationGroup>
            <configurationGroup name="start_date">
              <configuration name="headers" type="delimited" locale="fr_ML">
                <value>default:Date de début</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="end_date">
              <configuration name="headers" type="delimited" locale="fr_ML">
                <value>default:Date de fin</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="recurrence">
              <configuration name="headers" type="delimited" locale="fr_ML">
                <value>default:Fréquence de Répétition</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>