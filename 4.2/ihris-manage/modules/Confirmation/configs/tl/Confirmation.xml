<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="ihris-manage-Confirmation" path="/I2CE">
  <metadata>
    <version>4.3.0.1</version>
  </metadata>
  <configurationGroup name="ihris-manage-Confirmation" path="/I2CE" locale="tl">
    <version>4.3.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="tl">
      <configuration name="person_can_view_child_form_confirmation" locale="tl">
        <value>Makikita ang pormularyo ng pagpapatunay sa bata ng isang tao</value>
      </configuration>
      <configuration name="person_can_edit_child_form_confirmation" locale="tl">
        <value>Mababago ang mga pormularyo ng pagpapatunay sa bata ng isang tao</value>
      </configuration>
      <configuration name="can_edit_database_list_confirmation_type" locale="tl">
        <value>Mababago ang talaan ng kalipunan ng dato ng uri ng pagpapatunay</value>
      </configuration>
      <configuration name="can_view_database_list_confirmation_type" locale="tl">
        <value>Makikita ang talaan ng kalipunan ng dato ng uri ng pagpapatunay</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="confirmation">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="tl">
            <value>Magdagdag/Isapanahon ang Paniniyak</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="confirmation">
          <configuration name="display" values="single" locale="tl">
            <value>Paniniyak</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="confirmation_type">
          <configuration name="display" values="single" locale="tl">
            <value>Uri ng Pagtitiyak</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_ConfirmationType">
          <configurationGroup name="fields">
            <configurationGroup name="probation_period">
              <configuration name="headers" type="delimited" locale="tl">
                <value>default:Panahon ng Pagsubok (Mga Buwan)</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="job">
              <configuration name="headers" type="delimited" locale="tl">
                <value>default:Kaugnay na Hanapbuhay</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="name">
              <configuration name="headers" type="delimited" locale="tl">
                <value>default:Pangalan</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="iHRIS_Confirmation">
          <configurationGroup name="fields">
            <configurationGroup name="valid">
              <configuration name="headers" type="delimited" locale="tl">
                <value>default:Katanggap-tanggap</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="confirmation_type">
              <configuration name="headers" type="delimited" locale="tl">
                <value>default:Uri ng Pagtitiyak</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="date">
              <configuration name="headers" type="delimited" locale="tl">
                <value>default:Petsa</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>