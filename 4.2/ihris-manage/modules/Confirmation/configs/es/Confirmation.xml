<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="ihris-manage-Confirmation" path="/I2CE">
  <metadata>
    <version>4.3.0.1</version>
  </metadata>
  <configurationGroup name="ihris-manage-Confirmation" path="/I2CE" locale="es">
    <version>4.3.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="es">
      <configuration name="person_can_view_child_form_confirmation" locale="es">
        <value>Puede Visualizar el sub-formulario de Confirmación de una Persona</value>
      </configuration>
      <configuration name="person_can_edit_child_form_confirmation" locale="es">
        <value>Puede Editar el sub-formulario de Confirmación de una Persona</value>
      </configuration>
      <configuration name="can_edit_database_list_confirmation_type" locale="es">
        <value>Puede Editar la base de datos de tipos de Confirmación</value>
      </configuration>
      <configuration name="can_view_database_list_confirmation_type" locale="es">
        <value>Puede Visalizar la base de datos de tipos de Confirmación</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="confirmation">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="es">
            <value>Añadir / Actualizar Confirmación</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="confirmation">
          <configuration name="display" values="single" locale="es">
            <value>Confirmación</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="confirmation_type">
          <configuration name="display" values="single" locale="es">
            <value>Tipos de Confirmación</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_ConfirmationType">
          <configurationGroup name="fields">
            <configurationGroup name="probation_period">
              <configuration name="headers" type="delimited" locale="es">
                <value>default:Período de Prueba</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="job">
              <configuration name="headers" type="delimited" locale="es">
                <value>default:Trabajo Asociad</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="name">
              <configuration name="headers" type="delimited" locale="es">
                <value>default:Nombre</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="iHRIS_Confirmation">
          <configurationGroup name="fields">
            <configurationGroup name="valid">
              <configuration name="headers" type="delimited" locale="es">
                <value>default:Válido</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="confirmation_type">
              <configuration name="headers" type="delimited" locale="es">
                <value>default:Tipos de Confirmación</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="date">
              <configuration name="headers" type="delimited" locale="es">
                <value>default:Fecha</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>