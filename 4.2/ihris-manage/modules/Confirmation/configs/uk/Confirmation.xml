<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="ihris-manage-Confirmation" path="/I2CE">
  <metadata>
    <version>4.3.0.1</version>
  </metadata>
  <configurationGroup name="ihris-manage-Confirmation" path="/I2CE" locale="uk">
    <version>4.3.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="uk">
      <configuration name="person_can_view_child_form_confirmation" locale="uk">
        <value>Можна переглядати дочірню форму підтвердження особи</value>
      </configuration>
      <configuration name="person_can_edit_child_form_confirmation" locale="uk">
        <value>Можна редагувати дочірню форму підтвердження особи</value>
      </configuration>
      <configuration name="can_edit_database_list_confirmation_type" locale="uk">
        <value>Можна змінити тип підтвердження списку баз даних</value>
      </configuration>
      <configuration name="can_view_database_list_confirmation_type" locale="uk">
        <value>Можна переглянути тип підтвердження списку баз даних</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="confirmation">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="uk">
            <value>Додати/оновити підтвердження</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="confirmation">
          <configuration name="display" values="single" locale="uk">
            <value>Підтвердження</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="confirmation_type">
          <configuration name="display" values="single" locale="uk">
            <value>Тип підтвердження</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_ConfirmationType">
          <configurationGroup name="fields">
            <configurationGroup name="probation_period">
              <configuration name="headers" type="delimited" locale="uk">
                <value>default:Випробувальний термін (місяців)</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="job">
              <configuration name="headers" type="delimited" locale="uk">
                <value>default:Пов'язана робота</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="name">
              <configuration name="headers" type="delimited" locale="uk">
                <value>default:Назва</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="iHRIS_Confirmation">
          <configurationGroup name="fields">
            <configurationGroup name="valid">
              <configuration name="headers" type="delimited" locale="uk">
                <value>default:Дійсний</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="confirmation_type">
              <configuration name="headers" type="delimited" locale="uk">
                <value>default:Тип підтвердження</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="date">
              <configuration name="headers" type="delimited" locale="uk">
                <value>default:Дата</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>