<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="ihris-manage-Application" path="/I2CE">
  <metadata>
    <version>4.3.0.1</version>
  </metadata>
  <configurationGroup name="ihris-manage-Application" path="/I2CE" locale="et">
    <version>4.3.0.1</version>
    <configurationGroup name="invalid_formfield_message" path="/modules/forms/formfield_messages" locale="et">
      <configuration name="app_felon" path="application/felony_circumstances/required"></configuration>
      <configuration name="app_hours" path="application/hours/required"></configuration>
    </configurationGroup>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="et">
      <configuration name="person_can_view_child_form_position_decision" locale="et">
        <value>Võib vaadata isiku positsiooni otsuse alamvormi</value>
      </configuration>
      <configuration name="person_can_edit_child_form_position_decision" locale="et">
        <value>Võib redigeerida isiku positsiooni otsuse alamvormi</value>
      </configuration>
      <configuration name="person_can_view_child_form_position_interview" locale="et">
        <value>Võib vaadata isiku positsiooni intervjuu alamvormi</value>
      </configuration>
      <configuration name="person_can_edit_child_form_position_interview" locale="et">
        <value>Võib redigeerida isiku positsiooni otsuse alamvormi</value>
      </configuration>
      <configuration name="person_can_view_child_form_application" locale="et"></configuration>
      <configuration name="person_can_edit_child_form_application" locale="et"></configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="application">
          <configuration name="display" values="single" locale="et">
            <value>Taotleja</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="position_interview">
          <configuration name="display" values="single" locale="et">
            <value>Positsiooni intervjuu</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="position_decision">
          <configuration name="display" values="single" locale="et">
            <value>Positsiooni otsus</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_Applicant">
          <configurationGroup name="fields">
            <configurationGroup name="position">
              <configuration name="headers" type="delimited" locale="et">
                <value>default:Positsioon(id)</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="felony">
              <configuration name="headers" type="delimited" locale="et">
                <value>default:Kas oled kunagi kuriteos süüdi mõistetud?</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="felony_circumstance">
              <configuration name="headers" type="delimited" locale="et">
                <value>default:Kui jah, selgita olukorrast.</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="other_info">
              <configuration name="headers" type="delimited" locale="et">
                <value>default:Lisaks sinu töö ajaloole, kas on muid oskusi, kvalifikatsioone või kogemusi mida peaksime arvestama?</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="hear">
              <configuration name="headers" type="delimited" locale="et">
                <value>default:Kuidas said sellest avatud positsioonist teada?</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="start_date">
              <configuration name="headers" type="delimited" locale="et">
                <value>default:Millal võid alustada?</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="desired_wage">
              <configuration name="headers" type="delimited" locale="et">
                <value>default:Soovitud palk</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="full_time">
              <configuration name="headers" type="delimited" locale="et">
                <value>default:Kas otsite täiskohaga tööd?</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="hours">
              <configuration name="headers" type="delimited" locale="et">
                <value>default:Kui ei, mis tundidel olete saadaval?</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="iHRIS_PositionInterview">
          <configurationGroup name="fields">
            <configurationGroup name="date">
              <configuration name="headers" type="delimited" locale="et">
                <value>default:Intervjuu kuupäev</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="person">
              <configuration name="headers" type="delimited" locale="et">
                <value>default:Kohalolijad</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="comments">
              <configuration name="headers" type="delimited" locale="et">
                <value>default:Kommentaarid</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="iHRIS_PositionDecision">
          <configurationGroup name="fields">
            <configurationGroup name="date">
              <configuration name="headers" type="delimited" locale="et">
                <value>default:Otsuse kuupäev</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="offer">
              <configuration name="headers" type="delimited" locale="et">
                <value>default:Saadad tööpakkumise?</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="comments">
              <configuration name="headers" type="delimited" locale="et">
                <value>default:Kommentaarid</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="application">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="et">
            <value>Lisa/Uuenda taotlus</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="position_decision">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="et">
            <value>Lisa/Uuenda positsiooni otsus</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="position_interview">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="et">
            <value>Lisa/Uuenda positsiooni intervjuu</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>