<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="ihris-manage-Application" path="/I2CE">
  <metadata>
    <version>4.3.0.1</version>
  </metadata>
  <configurationGroup name="ihris-manage-Application" path="/I2CE" locale="fr">
    <version>4.3.0.1</version>
    <configurationGroup name="invalid_formfield_message" path="/modules/forms/formfield_messages" locale="fr">
      <configuration name="app_felon" path="application/felony_circumstances/required">
        <value>Vous avez sélectionné que vous êtes convaincu d'un forfait, mais vous ne décrivez pas les circonstances.</value>
      </configuration>
      <configuration name="app_hours" path="application/hours/required">
        <value>Vous avez sélectionné pas à temps complet, mais vous n'avez pas fourni les heures auxquelles vous êtes disponible.</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="fr">
      <configuration name="person_can_view_child_form_position_decision" locale="fr">
        <value>Peut afficher le sous-formulaire de la Décision du Poste d’une personne</value>
      </configuration>
      <configuration name="person_can_edit_child_form_position_decision" locale="fr">
        <value>Peut éditer le sous-formulaire de la Décision du Poste d’une personne</value>
      </configuration>
      <configuration name="person_can_view_child_form_position_interview" locale="fr">
        <value>Peut afficher le sous-formulaire de l’Entretien du Poste d’une personne</value>
      </configuration>
      <configuration name="person_can_edit_child_form_position_interview" locale="fr">
        <value>Peut éditer le sous-formulaire de l’entretien du poste d’une personne</value>
      </configuration>
      <configuration name="person_can_view_child_form_application" locale="fr">
        <value>Impossible de visionner l'application de formulaires d'enfant d'une personne</value>
      </configuration>
      <configuration name="person_can_edit_child_form_application" locale="fr">
        <value>Impossible de modifier l'application de formulaires d'enfant d'une personne</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="application">
          <configuration name="display" values="single" locale="fr">
            <value>Candidat</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="position_interview">
          <configuration name="display" values="single" locale="fr">
            <value>Entretien relatif au poste</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="position_decision">
          <configuration name="display" values="single" locale="fr">
            <value>Décision concernant le poste</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_Applicant">
          <configurationGroup name="fields">
            <configurationGroup name="position">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:Poste(s)</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="felony">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:Avez vous déjà été reconnu coupable d’un crime?</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="felony_circumstance">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:Si oui, donnez les circonstances.</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="other_info">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:En plus de vos antécédents professionnels, y a t-il d’autres compétences, qualifications ou expérience que nous devrions prendre en compte?</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="hear">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:Comment avez-vous pris connaissance de cette vacance de poste?</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="start_date">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:Quand pouvez-vous débuter?</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="desired_wage">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:Prétentions salariales</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="full_time">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:Recherchez-vous un emploi à temps plein?</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="hours">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:Si non, quelles sont les heures auxquelles vous êtes disponible?</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="iHRIS_PositionInterview">
          <configurationGroup name="fields">
            <configurationGroup name="date">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:Date de l’entretien</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="person">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:Personnes présentes</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="comments">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:Commentaires</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="iHRIS_PositionDecision">
          <configurationGroup name="fields">
            <configurationGroup name="date">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:Date de la décision</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="offer">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:Faire une offre d’emploi?</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="comments">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:Commentaires</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="application">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="fr">
            <value>Ajouter/Actualiser une candidature</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="position_decision">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="fr">
            <value>Ajouter/Actualiser une décision de poste</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="position_interview">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="fr">
            <value>Ajouter/Actualiser un entretien pour un poste</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>