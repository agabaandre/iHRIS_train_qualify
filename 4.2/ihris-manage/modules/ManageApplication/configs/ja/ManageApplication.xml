<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="ihris-manage-Application" path="/I2CE">
  <metadata>
    <version>4.2.2.0.1</version>
  </metadata>
  <configurationGroup name="ihris-manage-Application" path="/I2CE" locale="ja">
    <version>4.2.0.1</version>
    <configurationGroup name="invalid_formfield_message" path="/modules/forms/formfield_messages" locale="ja">
      <configuration name="app_felon" path="application/felony_circumstances/required">
        <value>重罪の有罪判決を受けた状況を説明していません。</value>
      </configuration>
      <configuration name="app_hours" path="application/hours/required"></configuration>
    </configurationGroup>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="ja">
      <configuration name="person_can_view_child_form_position_decision" locale="ja">
        <value>役職決定の子フォームを表示できます。</value>
      </configuration>
      <configuration name="person_can_edit_child_form_position_decision" locale="ja">
        <value>役職決定の子フォームを編集できます。</value>
      </configuration>
      <configuration name="person_can_view_child_form_position_interview" locale="ja">
        <value>役職面接の子フォームを表示できます。</value>
      </configuration>
      <configuration name="person_can_edit_child_form_position_interview" locale="ja">
        <value>役職面接の子フォームを編集できます。</value>
      </configuration>
      <configuration name="person_can_view_child_form_application" locale="ja"></configuration>
      <configuration name="person_can_edit_child_form_application" locale="ja"></configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="application">
          <configuration name="display" values="single" locale="ja">
            <value>志願者</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="position_interview">
          <configuration name="display" values="single" locale="ja">
            <value>役職面接</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="position_decision">
          <configuration name="display" values="single" locale="ja">
            <value>役職決定</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_Applicant">
          <configurationGroup name="fields">
            <configurationGroup name="position">
              <configuration name="headers" type="delimited" locale="ja">
                <value>default:役職</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="felony">
              <configuration name="headers" type="delimited" locale="ja">
                <value>default:これまでに重罪を犯し有罪判決を受けたことがありますか？</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="felony_circumstance">
              <configuration name="headers" type="delimited" locale="ja">
                <value>default:Yes の場合、状況を述べて下さい。</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="other_info">
              <configuration name="headers" type="delimited" locale="ja">
                <value>default:職歴に加えて、我々が考慮すべき他のスキル、資格や経験がありますか？</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="hear">
              <configuration name="headers" type="delimited" locale="ja">
                <value>default:この空きポストをどのように知りましたか？</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="start_date">
              <configuration name="headers" type="delimited" locale="ja">
                <value>default:いつから開始できますか？</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="desired_wage">
              <configuration name="headers" type="delimited" locale="ja">
                <value>default:希望給与</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="full_time">
              <configuration name="headers" type="delimited" locale="ja">
                <value>default:フルタイムの雇用を探していますか？</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="hours">
              <configuration name="headers" type="delimited" locale="ja">
                <value>default:No の場合、就業可能な時間帯は？</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="iHRIS_PositionInterview">
          <configurationGroup name="fields">
            <configurationGroup name="date">
              <configuration name="headers" type="delimited" locale="ja">
                <value>default:面接日</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="person">
              <configuration name="headers" type="delimited" locale="ja">
                <value>default:参加者</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="comments">
              <configuration name="headers" type="delimited" locale="ja">
                <value>default:コメント</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="iHRIS_PositionDecision">
          <configurationGroup name="fields">
            <configurationGroup name="date">
              <configuration name="headers" type="delimited" locale="ja">
                <value>default:決定日</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="offer">
              <configuration name="headers" type="delimited" locale="ja">
                <value>default:仕事のオファーをしますか？</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="comments">
              <configuration name="headers" type="delimited" locale="ja">
                <value>default:コメント</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="application">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="ja">
            <value>アプリケーションの追加と更新</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="position_decision">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="ja">
            <value>職位決定の追加/更新</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="position_interview">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="ja">
            <value>職位面接の追加/更新</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>