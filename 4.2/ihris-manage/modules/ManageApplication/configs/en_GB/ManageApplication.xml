<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="ihris-manage-Application" path="/I2CE">
  <metadata>
    <version>4.2.2.0.1</version>
  </metadata>
  <configurationGroup name="ihris-manage-Application" path="/I2CE" locale="en_GB">
    <version>4.2.0.1</version>
    <configurationGroup name="invalid_formfield_message" path="/modules/forms/formfield_messages" locale="en_GB">
      <configuration name="app_felon" path="application/felony_circumstances/required">
        <value>You told us that you were convicted of a crime, but you didn't describe the circumstances.</value>
      </configuration>
      <configuration name="app_hours" path="application/hours/required">
        <value>You selected an option other than full time, but you didn't tell us which hours you are available.</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="en_GB">
      <configuration name="person_can_view_child_form_position_decision" locale="en_GB"></configuration>
      <configuration name="person_can_edit_child_form_position_decision" locale="en_GB"></configuration>
      <configuration name="person_can_view_child_form_position_interview" locale="en_GB"></configuration>
      <configuration name="person_can_edit_child_form_position_interview" locale="en_GB"></configuration>
      <configuration name="person_can_view_child_form_application" locale="en_GB"></configuration>
      <configuration name="person_can_edit_child_form_application" locale="en_GB"></configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="application">
          <configuration name="display" values="single" locale="en_GB"></configuration>
        </configurationGroup>
        <configurationGroup name="position_interview">
          <configuration name="display" values="single" locale="en_GB"></configuration>
        </configurationGroup>
        <configurationGroup name="position_decision">
          <configuration name="display" values="single" locale="en_GB"></configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_Applicant">
          <configurationGroup name="fields">
            <configurationGroup name="position">
              <configuration name="headers" type="delimited" locale="en_GB"></configuration>
            </configurationGroup>
            <configurationGroup name="felony">
              <configuration name="headers" type="delimited" locale="en_GB">
                <value>default:Have you ever been convicted of a crime?</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="felony_circumstance">
              <configuration name="headers" type="delimited" locale="en_GB"></configuration>
            </configurationGroup>
            <configurationGroup name="other_info">
              <configuration name="headers" type="delimited" locale="en_GB"></configuration>
            </configurationGroup>
            <configurationGroup name="hear">
              <configuration name="headers" type="delimited" locale="en_GB"></configuration>
            </configurationGroup>
            <configurationGroup name="start_date">
              <configuration name="headers" type="delimited" locale="en_GB"></configuration>
            </configurationGroup>
            <configurationGroup name="desired_wage">
              <configuration name="headers" type="delimited" locale="en_GB"></configuration>
            </configurationGroup>
            <configurationGroup name="full_time">
              <configuration name="headers" type="delimited" locale="en_GB"></configuration>
            </configurationGroup>
            <configurationGroup name="hours">
              <configuration name="headers" type="delimited" locale="en_GB"></configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="iHRIS_PositionInterview">
          <configurationGroup name="fields">
            <configurationGroup name="date">
              <configuration name="headers" type="delimited" locale="en_GB"></configuration>
            </configurationGroup>
            <configurationGroup name="person">
              <configuration name="headers" type="delimited" locale="en_GB"></configuration>
            </configurationGroup>
            <configurationGroup name="comments">
              <configuration name="headers" type="delimited" locale="en_GB"></configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="iHRIS_PositionDecision">
          <configurationGroup name="fields">
            <configurationGroup name="date">
              <configuration name="headers" type="delimited" locale="en_GB"></configuration>
            </configurationGroup>
            <configurationGroup name="offer">
              <configuration name="headers" type="delimited" locale="en_GB"></configuration>
            </configurationGroup>
            <configurationGroup name="comments">
              <configuration name="headers" type="delimited" locale="en_GB"></configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="application">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="en_GB"></configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="position_decision">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="en_GB"></configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="position_interview">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="en_GB"></configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>