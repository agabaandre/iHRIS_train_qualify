<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="ihris-manage-Application" path="/I2CE">
  <metadata>
    <version>4.3.0.1</version>
  </metadata>
  <configurationGroup name="ihris-manage-Application" path="/I2CE" locale="cs">
    <version>4.3.0.1</version>
    <configurationGroup name="invalid_formfield_message" path="/modules/forms/formfield_messages" locale="cs">
      <configuration name="app_felon" path="application/felony_circumstances/required">
        <value>Vybrali jste, že jste byli odsouzeni za trestný čin, ale nepopsali jste okolnosti.</value>
      </configuration>
      <configuration name="app_hours" path="application/hours/required">
        <value>Vybrali jste Ne na plný úvazek, ale nezadali jste v kterých hodinách jste dostupní.</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="cs">
      <configuration name="person_can_view_child_form_position_decision" locale="cs">
        <value>Může zobrazit podřazený formulář rozhodnutí o pozici pro osobu</value>
      </configuration>
      <configuration name="person_can_edit_child_form_position_decision" locale="cs">
        <value>Může upravit podřazený formulář rozhodnutí o pozici pro osobu</value>
      </configuration>
      <configuration name="person_can_view_child_form_position_interview" locale="cs">
        <value>Může zobrazit podřazený formulář pohovoru o pozici pro osobu</value>
      </configuration>
      <configuration name="person_can_edit_child_form_position_interview" locale="cs">
        <value>Může upravit podřazený formulář pohovoru o pozici pro osobu</value>
      </configuration>
      <configuration name="person_can_view_child_form_application" locale="cs">
        <value>Může zobrazit podřazené formuláře přihlášky osoby</value>
      </configuration>
      <configuration name="person_can_edit_child_form_application" locale="cs">
        <value>Může upravit podřazené formuláře přihlášky osoby</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="application">
          <configuration name="display" values="single" locale="cs">
            <value>Uchazeč</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="position_interview">
          <configuration name="display" values="single" locale="cs">
            <value>Rozohovor o pozici</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="position_decision">
          <configuration name="display" values="single" locale="cs">
            <value>Rozhodnutí o pozici</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_Applicant">
          <configurationGroup name="fields">
            <configurationGroup name="position">
              <configuration name="headers" type="delimited" locale="cs">
                <value>default:Pozice</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="felony">
              <configuration name="headers" type="delimited" locale="cs">
                <value>default:Byli jste někdy odsouzeni za trestný čin?</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="felony_circumstance">
              <configuration name="headers" type="delimited" locale="cs">
                <value>default:Pokud ano, vysvětlete okolnosti.</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="other_info">
              <configuration name="headers" type="delimited" locale="cs">
                <value>default:Kromě Vaší pracovní historie, existují ještě nějaké jiné zkušenosti, kvalifikace, nebo zkušenosti, o kterých bychom měli vědět?</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="hear">
              <configuration name="headers" type="delimited" locale="cs">
                <value>default:Jak jste se o této volné pozici dozvěděli?</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="start_date">
              <configuration name="headers" type="delimited" locale="cs">
                <value>default:Kdy můžete začít?</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="desired_wage">
              <configuration name="headers" type="delimited" locale="cs">
                <value>default:Požadovaný plat</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="full_time">
              <configuration name="headers" type="delimited" locale="cs">
                <value>default:Hledáte zaměstnání na plný úvazek?</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="hours">
              <configuration name="headers" type="delimited" locale="cs">
                <value>default:Pokud ne, v kterých hodinách jste k dispozici?</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="iHRIS_PositionInterview">
          <configurationGroup name="fields">
            <configurationGroup name="date">
              <configuration name="headers" type="delimited" locale="cs">
                <value>default:Datum rozhovoru</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="person">
              <configuration name="headers" type="delimited" locale="cs">
                <value>default:Zúčastnění lidé</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="comments">
              <configuration name="headers" type="delimited" locale="cs">
                <value>default:Komentáře</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="iHRIS_PositionDecision">
          <configurationGroup name="fields">
            <configurationGroup name="date">
              <configuration name="headers" type="delimited" locale="cs">
                <value>default:Datum rozhodnutí</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="offer">
              <configuration name="headers" type="delimited" locale="cs">
                <value>default:Nabídnout práci?</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="comments">
              <configuration name="headers" type="delimited" locale="cs">
                <value>default:Komentáře</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="application">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="cs">
            <value>Přidat/Aktualizovat žádost</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="position_decision">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="cs">
            <value>Přidat/Aktualizovat rozhodnutí o pozici</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="position_interview">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="cs">
            <value>Přidat/Aktualizovat rozhovor o pozici</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>