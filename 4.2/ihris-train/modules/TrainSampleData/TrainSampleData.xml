<?xml version="1.0"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="ihris-train-sample-data">
  <metadata>
    <displayName>iHRIS Train Sample Data</displayName>
    <className>iHRIS_Module_Train_SampleData</className>
    <category>Site</category>
    <description>iHRIS Train Sample Data</description>
    <creator>Intrahealth Informatics</creator>
    <email>hris@capacityproject.org</email>
    <link>https://launchpad.net/ihris-manage</link>
    <version>4.2.1.0</version>
    <enable name="ihris-train-SampleData-cadre" />
    <enable name="ihris-train-SampleData-classification" />
    <enable name="ihris-train-SampleData-facility_type" />
    <enable name="ihris-train-SampleData-health_facility" />
    <enable name="ihris-train-SampleData-job" />
    <enable name="ihris-train-SampleData-council" />
    <enable name="ihris-train-SampleData-training_classification" />
    <enable name="ihris-train-SampleData-funder" />
    <path name="sql">
      <value>./sql</value>
    </path>
    <path name="modules">
      <value>./modules</value>
    </path>
  </metadata>
</I2CEConfiguration>